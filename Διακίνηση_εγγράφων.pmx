				if(is_array(@=docFiles[1]) and count(@=docFiles[1]) > 0 ){ // new version
			
					$nfileUid = PMFAddInputDocument($inDocId, '', @=docFiles[1]['fileUid'], 'INPUT', $comment , 'NV', $caseId, @%INDEX, '', $userId, 'file', $copy2file);
