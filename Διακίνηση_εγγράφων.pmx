        <dyn_content><![CDATA[{"name":"\u0395\u03b9\u03c3\u03b5\u03c1\u03c7\u03cc\u03bc\u03b5\u03bd\u03bf scan and insert files 3.4","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"724894992596268c03c7538062322529","name":"\u0395\u03b9\u03c3\u03b5\u03c1\u03c7\u03cc\u03bc\u03b5\u03bd\u03bf scan and insert files 3.4","description":"","mode":"edit","script":{"type":"js","code":"$(\"form\").css(\"background-color\", \"#DEE0EC\");\n$(\"#panelScan\").css(\"background-color\",\"#DEE0EC\");\n\n$(\".form-control\").css(\"font-size\", \"0.9em\");\n$(\".textlabel\").css(\"font-size\", \"0.88em\");\n$(\".title-column\").css(\"font-size\", \"0.88em\");\n$(\".pmdynaform-grid-title\").css(\"font-size\", \"0.9em\");\n\n\n$(\"#form\\\\[rcptsSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\n\n$(\"#title1\").css(\"color\",\"#ffffff\");\n$(\"#title1\").css(\"background-color\",\"#428bca\");\ndocument.getElementById('title1').style.borderRadius= \"8px\";\n\ndocument.getElementById(\"authorities\").style.padding = \"5px 10px 20px 10px\";\ndocument.getElementById('authorities').style.borderRadius= \"10px\";\ndocument.getElementById(\"gridDocFiles\").style.padding = \"5px 10px 20px 10px\";\ndocument.getElementById('gridDocFiles').style.borderRadius= \"10px\";\ndocument.getElementById(\"gridAttFiles\").style.padding = \"5px 10px 20px 10px\";\ndocument.getElementById('gridAttFiles').style.borderRadius= \"10px\";\n\n\n$(\"#form\\\\[rcptsSubmit\\\\]\").css(\"text-align\",\"center\");\n\n\n$(\"#gridDocFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#gridDocFiles-body\").children().css(\"background-color\",\"#CCD8CC\");\n$(\"#gridAttFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#gridAttFiles-body\").children().css(\"background-color\",\"#CCD8CC\");\n\n$(\"#panelScanDocs\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n$(\"#authorities\").css(\"background-color\",\"#D8D8C0\");\n$(\"#authorities-body\").children().css(\"background-color\",\"#D8D8C0\");\n$(\"#bpmsUsers\").css(\"background-color\",\"#D8D8C0\");\n$(\"#bpmsUsers-body\").children().css(\"background-color\",\"#D8D8C0\");\n$(\"#gridBpmsRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridBpmsRcpts-body\").children().css(\"background-color\",\"#D8D8C0\");\n$(\"#gridOtherRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridOtherRcpts-body\").children().css(\"background-color\",\"#D8D8C0\");\n\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\".pmdynaform-control-checkbox-list\").css(\"background-color\",\"#D8D8C0\")\n\/\/\n\/\/\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) {\n    \t$(selector).show();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) {\n        $(\"#sxetikaButton\").show();\n        $(selector).hide();\n      }\n    }\n    else {\n    \t$(selector).hide();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) $(\"#sxetikaButton\").hide();\n    }\n  }\n}\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelDocFiles\");\npanelDisplay(\"panelScanDocs\");\n  \n\n\/\/\n\n$(\"#SCOPE\").hide();\n\n\/\/get form id\nvar formId = $(\"form\")[0].id;\nvar form = getFormById(formId);\n\n$(\"#reloadFormId\").setValue(formId);\n\n\/\/\n\n\/\/if($(\"#countRcpts\").getValue() == 1 ) $(\"#submit2\").hide();\n\n\n \n\n\n \n\n\n\n\n\n\/*\n\/\n\/ \u0394\u03b9\u03b1\u03c7\u03b5\u03af\u03c1\u03b9\u03c3\u03b7 \u03c4\u03c9\u03bd \u03c3\u03c5\u03bd\u03b7\u03bc\u03bc\u03ad\u03bd\u03c9\u03bd  \n\/  \n\/  \n*\/\n\n\n\nvar aTrace  = [];\n\taTrace[1]  = {fCatA:-1,insOrUpd:0};\n \n\n  \n\/\/\n\/\/\n  \nfunction setGridEvents(){\n\t\n  var nGridRows = $(\"#gridAttFiles\").getNumberRows();\n  \n  var $fCatA ;\n  var $fCatA_label ;\n    \n  if(nGridRows !== 0){\n    \n\n    \n    \t\/\/remove first events\n       for (var i = 1; i <= nGridRows; i++) {\n\n\n          \t$fCatA_label =  $(\"#form\\\\[gridAttFiles\\\\]\\\\[\"+i+\"\\\\]\\\\[fCatA_label\\\\]\");\n            \n          \t$fCatA_label.off('click focusout change');\n            \n        }\n        \n\n        \/\/add events\n        for (var i = 1; i <= nGridRows; i++) {\n          \n  \t\t\t$fCatA =  $(\"#form\\\\[gridAttFiles\\\\]\\\\[\"+i+\"\\\\]\\\\[fCatA\\\\]\");\n          \t$fCatA_label =  $(\"#form\\\\[gridAttFiles\\\\]\\\\[\"+i+\"\\\\]\\\\[fCatA_label\\\\]\");\n \t\t\t \n\n  \n  \t\t\t$fCatA_label.change({ gridRow: i},function(event) {\n              \t\t\t\n          \n      \t\t\t\t\tcatAttMngmnt(\"gridAttFiles\",event.data.gridRow, \"fCatA\");\n              \t\t\t\n    \t\t});\n          \n\n            $fCatA_label.click({ gridRow: i},function(event) {\n            \n     \t\t\tif( $(\"#gridAttFiles\").getValue(event.data.gridRow,2).indexOf($fCatA_label.val()) == -1 ) {\n     \t\t\t\taTrace[event.data.gridRow][\"fCatA\"]  =  $(\"#gridAttFiles\").getValue(event.data.gridRow,2);\n       \t\t\t\t\n       \t\t\t\t$(\"#gridAttFiles\").setValue(0,event.data.gridRow,3); \/\/no ins or upd\n       \t\t\t\n            \t}\n       \n          \t});  \n          \n \n        }\n    \n    }\n\n} \/\/ function\n\n\n\n\n\/\/\nfunction catAttMngmnt(grid,line, fldNameId) {\n    \n    var c; \/\/for confirm   \n  \n    var gridfld_label = \"#form\\\\[\"+grid+\"\\\\]\\\\[\"+line+\"\\\\]\\\\[\"+fldNameId+\"_label\\\\]\";\n \tvar chngdfldName = $(gridfld_label).val(); \n  \n    $(\"#senderIsSelected\").setValue(1);\n    \n    \n   if($(\"#\"+grid).getValue(line,2).length == 0) {\n      aTrace[line][fldNameId] = -1; \/\/reset previous selected authority\n\n      $(\"#\"+grid).setValue(0,line,3); \/\/\u03b5\u03be\u03ad\u03c4\u03b1\u03c3\u03b7 \u03b5\u03bd\u03ad\u03c1\u03b3\u03b5\u03b9\u03b1\u03c2(insert=1 or update=2 or nothing=0) \u03b1\u03bd\u03ac \u03b3\u03c1\u03b1\u03bc