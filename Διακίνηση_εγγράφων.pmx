					@=docFiles[1] = array(); // reset main file possition
			
				}
	
				if($nfileUid != "") {
				 
					unlink($copy2file);
			
					$oAppDoc = new AppDocument();
			
					$oContent = $oAppDoc->load($nfileUid); // load new version with ADA
			
