				<th>Πρωτόκολλο</th>
				<th>Τύπος εγγράφου</th>
    			<th>Θέμα</th>
   			</tr> ";

	if(isset($panelta) and strlen($panelta) > 10 ) 
		$panelta = "<tr><td><br><p><b>Συνημμένα</b></p><td><tr>". $panelta;

	@@panelSxtFiles .= $paneltd . $panelta. "</table>";	

@@panelSxtFiles .= "
