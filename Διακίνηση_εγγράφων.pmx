        <tri_webbot><![CDATA[//2.0 17/8/2016
//ενημέρωση του πίνακα παραληπτών με τους αρχικούς παραλήτες και αποστολέα του εγγράφου


$grpId = @@protScopeGroupUid;

// πρώτα εξατάζουμε τους αποστολείς
//
if(strlen(@@INC_SENDER) == 32 ) {
	@=gridBpmsRcpts['rcptDep'] = @@INC_SENDER;
	@=gridBpmsRcpts['rcptDep_label'] = @@INC_SENDER_STR;
	@=gridBpmsRcpts['rcptType'] = "TO";
	@=gridBpmsRcpts['rcptDep_label'] = "Προς";
} else {
	if(strlen(@@INC_SENDER) < 32 ) {
		@=gridOtherRcpts['cNameId'] = @@INC_SENDER;
		@=gridOtherRcpts['cNameId_label'] = @@INC_SENDER_STR;
		@=gridOtherRcpts['cRcptType'] = "TO";
		@=gridOtherRcpts['cRcptType_label'] = "Προς";
	}
}


foreach(@=rcpts['bpms'][$grpId] as $deps) {
	if(!empty($deps) or count($deps) > 0 ) {
		foreach( $deps as $aMangr => $rcptType ) {
			$aMngr = key($aDep);
			$aMngrInfo = PMFInformationUser($aMngr);
			@=gridBpmsRcpts['rcptDep'] = $aMngr;
			@=gridBpmsRcpts['rcptDep_label'] = $aMngrInfo['departmentname']
			@=gridBpmsRcpts['rcptType'] = "CC";
			@=gridBpmsRcpts['rcptDep_label'] = "Κοινοποίηση";
		}
	
	}
}

foreach(@=rcpts['others']['send'] as $rcpts) {
	if(!empty($rcpts) or count($rcpts) > 0 ) {
		foreach( $rcpts as $aRcpt => $rcptType ) {
		
			$res = executeQuery("SELECT CONTACT_NAME FROM PMT_CONTACT WHERE CONTACT_ID = '$aRcpt'");
	 		@=gridOtherRcpts['cNameId'] = $aRcpt;
			@=gridOtherRcpts['cNameId_label'] = $res[1]
			@=gridOtherRcpts['cRcptType'] = "CC";
			@=gridOtherRcpts['cRcptType_label'] = "Κοινοποίηση";
		
		}
	
	}
}
 

