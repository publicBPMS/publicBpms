        <tri_webbot><![CDATA[#v.2.0 12/8/2016
#  Αναθέσεις χωρίς διαδικασία πρωτοκόλλου(Προϊστέμενοι, Εισηγητές)
#δρομολόγιση με newCase  
#
# Script TASK 'likes' to being run as prevUserUid user
$pU =@@prevUserUid;
$app = @@APPLICATION;
$indx = @@INDEX;

//executeQuery("UPDATE APP_CACHE_VIEW SET USR_UID = '$pU' WHERE APP_UID='$app' AND DEL_INDEX = '$indx'");
#


if(@%depLevel < 5 ) {
	if (!isset(@@onceRcpts) or @%onceRcpts != 1)   { 
		$protGrps = @=rcpts['bpms'];
		@=depUsers = array();
			
		if(count($protGrps) > 0 ) { 
			foreach($protGrps as $pGrp => $depUsers ) {
		
				foreach ($depUsers as $aUser => $propUser) {
		
					@=depUsers  = @=depUsers + array($aUser => $pGrp);
				}
	
			}
			reset(@=depUsers); 
		}
		@@onceRcpts = 1;

	} 
}
else { // depLeve >=5 
	
	if(@%onceSend != 1 or !isset(@@onceSend) or empty(@@onceSend)) { //δημιουργία πίνακα παραληπτών απαξ
		
		@=depUsers = array();
		@@onceSend = 1;	
		
		if(count(@=assign) >0  ){ // τουλάχιστον υπάρχει παραλήπτης 
			
			@=depUsers  = @=assign;
			
			
		}
	}
}


//
//

$pF = @=protFields;
extract($pF);
$prev_PROT_ID_STR = $PROT_ID_STR;
//
//
@@moreAssign = 0;
if(is_array(@=depUsers) and count(@=depUsers) > 0) { // υπάρχουν παραλήπτες
	@@moreAssign = 1;
	
	@@nextUserToBeAssigned = key(@=depUsers);
	$protSGUid = @=depUsers[@@nextUserToBeAssigned];
	
	array_shift(@=depUsers);	
	if(is_array(@=depUsers) and count(@=depUsers) == 0) @@moreAssign = 0;

//
//
// protokollo compact gia κάθε παραλήπτη DEPlEVEL <5
	if(@%depLevel < 5 ) {
		$aUser = @@nextUserToBeAssigned;
			 	
		$PROT_ID =	@=rcpts['bpms'][$protSGUid][$aUser]['protId'] ;
