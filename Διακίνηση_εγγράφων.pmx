\"#countRcpts\").getValue() == 1 ) $(\"#submit2\").hide();\n\n$(\"#gridAttFiles\").hide();\n$(\"#synimmenaButton\").click(function(){$(\"#gridAttFiles\").toggle();});\n\n\/\/\n$(\"#sxetikaButton\").click(function(){$(\"#panelSxtFiles\").toggle();});\n\/\/\n\n$(\"#chgrpBpmsTo\").show();\n$(\"#chgrpBpmsCc\").show();\n\n\/\/\n\n\n$(\"#protSubmit\").click(function(){$(\"#epilogiProtSupporter\").setValue(2);});  \/\/2 \u03a0\u03c1\u03c9\u03c4\u03bf\u03ba\u03cc\u03bb\u03bb\u03b7\u03c3\u03b7\n\n\/\/\n\n\n\n$(\"#drpdnBpmsTo\").hide(); \/\/ \u03c0\u03b1\u03bd\u03c4\u03b1 \u03ba\u03c1\u03c5\u03c6\u03cc. \u0395\u03bd\u03b1\u03c2 \u03c4\u03c1\u03cc\u03c0\u03bf\u03c2 \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03c0\u03ad\u03c1\u03bd\u03c9 \u03b1\u03c0\u03bf\u03c4\u03b5\u03bb\u03ad\u03bc\u03b1\u03c4\u03b1 SQL \u03b3\u03b9\u03b1 \u03c4\u03bf check group\n$(\"#drpdnBpmsCc\").hide(); \/\/ \u03c0\u03b1\u03bd\u03c4\u03b1 \u03ba\u03c1\u03c5\u03c6\u03cc. \u0395\u03bd\u03b1\u03c2 \u03c4\u03c1\u03cc\u03c0\u03bf\u03c2 \u03b3\u03b9\u03b1 \u03bd\u03b1 \u03c0\u03ad\u03c1\u03bd\u03c9 \u03b1\u03c0\u03bf\u03c4\u03b5\u03bb\u03ad\u03bc\u03b1\u03c4\u03b1 SQL \u03b3\u03b9\u03b1 \u03c4\u03bf check group\n$(\"#chgrpSelectedBpmsTo\").hide();\n$(\"#chgrpSelectedBpmsCc\").hide();\n\n\n\/*\nfunction chgrpsChanged(chgrpSet,chgrpUnset) {\n  console.log('chgrpsChanged=',chgrpSet);\n  \tvar setArrValues = $(\"#\"+chgrpUnset).getValue();\n  \n\/\/    $(\"#\"+chgrpSet).getValue().forEach(function(i, item){$(\"#form\\\\[\"+chgrpUnset+\"\\\\]\\\\[\"+i+\"\\\\]\").removeAttr('checked')});\n\n  \n  \t$(\"#\"+chgrpSet).getValue().forEach(function(i, item){\n    \n      \tsetArrValues = $.grep(setArrValues, function(j, jtem){\n        \n        \treturn(i !== j);\n      \t\n      \t});\n     $(\"#\"+chgrpUnset).setValue(setArrValues);\n    });\n  \n\n  if($(\"#\"+chgrpSet).attr('id').indexOf('chgrpBpms') > -1 ) { \/\/checkgrpoup \u03c4\u03c9\u03bd \u0394\u03b9\u03b5\u03c5\u03b8\u03cd\u03bd\u03c3\u03b5\u03c9\u03bd \u03c0\u03c1\u03c9\u03c4\u03bf\u03ba\u03cc\u03bb\u03bb\u03b7\u03c3\u03b7\u03c2\n  \tsCount = $(\"#\"+chgrpSet).getValue().length + $(\"#\"+chgrpUnset).getValue().length;\n  \t$(\"#countRRcpts\").setValue(sCount);\n  }\n}\n\n*\/\nfunction chgrpsDelEqls(chgrpSet,chgrpUnset, newSetValue) {\n  \n  \tvar chgrpUnsetValues = $(\"#\"+chgrpUnset).getValue();\n  \n\/\/    $(\"#\"+chgrpSet).getValue().forEach(function(i, item){$(\"#form\\\\[\"+chgrpUnset+\"\\\\]\\\\[\"+i+\"\\\\]\").removeAttr('checked')});\n  \t\n  $(\"#\"+chgrpSet).getValue().forEach(function(i, item){\n    \n      \tchgrpUnsetValues = $.grep(chgrpUnsetValues, function(j, jtem){\n        \n        \t\t\t\t\t\t\treturn(i !== j);\n      \t\n      \t\t\t\t\t\t\t});\n\n  });\n  \n  $(\"#\"+chgrpUnset).setValue(chgrpUnsetValues);\n\n  if($(\"#\"+chgrpSet).attr('id').indexOf('chgrpBpms') > -1 ) { \/\/checkgrpoup \u03c4\u03c9\u03bd \u0394\u03b9\u03b5\u03c5\u03b8\u03cd\u03bd\u03c3\u03b5\u03c9\u03bd \u03c0\u03c1\u03c9\u03c4\u03bf\u03ba\u03cc\u03bb\u03bb\u03b7\u03c3\u03b7\u03c2\n  \tsCount = $(\"#\"+chgrpSet).getValue().length + $(\"#\"+chgrpUnset).getValue().length;\n  \t$(\"#countRRcpts\").setValue(sCount);\n  }\n}\n\n\/\/\n\n\/*\n*\n* \u0394\u0399\u0391\u03a7\u0395\u0399\u03a1\u0399\u03a3\u0397 \u0391\u039d\u0391\u0398\u0395\u03a3\u0395\u03a9\u039d (chekgroupAssignBpms{To,Cc}  \u03c3\u03c4\u03bf\u03b9\u03c7\u03b5\u03af\u03b1 )\n* \n* \u03a0\u03b1\u03c1\u03b1\u03ba\u03bf\u03bb\u03bf\u03c5\u03b8\u03bf\u03cd\u03bc\u03b5 \u03c4\u03b9\u03c2 \u03b1\u03bb\u03bb\u03b1\u03b3\u03ad\u03c2 \u03c3\u03c4\u03b1 chgrpBpmsTo \u03ba\u03b1\u03b9chgrpBpmsCc \u03c3\u03c4\u03bf\u03b9\u03c7\u03b5\u03af\u03b1 ( \u03b4\u03b5\u03c2 \u03c0\u03b1\u03c1\u03b1\u03c0\u03ac\u03bd\u03c9 )\n*\/\n\n\n\nfunction chgrpAssignBpmsSetup(drpdnData,chckGrpId,mainChckGrp, chckMainSelected4drpbx) {\n\/\/ \u03b3\u03b9\u03b1 \u03ba\u03ac\u03b8\u03b5 \u03b1\u03bb\u03bb\u03b1\u03b3\u03ae \u03c3\u03c4\u03bf \u03ba\u03cd\u03c1\u03b9\u03bf checkgrp \u03b3\u03af\u03bd\u03b5\u03c4\u03b1\u03b9 reset \u03c4\u03bf assignment \u03ba\u03b1\u03b9 \u03cc\u03c4\u03b1\u03bd \u03b5\u03bd\u03b7\u03bc\u03b5\u03c1\u03c9\u03b8\u03b5\u03af \u03c4\u03bf dropbox, \u03b5\u03bd\u03b7\u03bc\u03b5\u03c1\u03ce\u03bd\u03b5\u03c4\u03b1\u03b9 \n\n  \n  $(\"#\"+chckGrpId).find('div.checkbox').remove();\n  $(\"#\"+chckMainSelected4drpbx).setValue('');\n  \n  if($(\"#\"+mainChckGrp).getValue().length > 0 || mainChckGrp.indexOf('Cc') > 0) {\n    \n     \tvar selectedBpms = $(\"#\"+mainChckGrp).getValue().join() ;\n     \n    \tif(mainChckGrp.indexOf('Cc') > 0) \n            selectedBpms = $(\"#\"+mainChckGrp).getValue().concat($(\"#chgrpBpmsTo\").getValue()).join();\n        \n    \t$(\"#\"+chckMainSelected4drpbx).setValue(selectedBpms);\n    \n  }\n  \n  \n}\n\n\/\/\n\n\n\nfunction drpdn2ChckGrp (drpdnData,chckGrpId){ \n \n  var drpdnOptions = $(\"#\"+drpdnData).getInfo().options; \/\/where we get data. \n  var chckGrpOptions = $(\"#\"+chckGrpId).getInfo().options; \/\/where we put data. \n  var chekboxEl;\n      \n  var oldValues = $(\"#\"+chckGrpId).getValue();\n  \n  \n  if(drpdnOptions.length > 0){\n   \n    chckGrpOptions[0] = {value:\"chckGrpSelAll\", label:'\u0395\u03c0\u03b9\u03bb\u03bf\u03b3\u03ae \u03cc\u03bb\u03c9\u03bd',selected:false};\n     \n  \tchekboxEl = \"<div class=\\\"checkbox\\\">\" +\n            \t\t\t\t\t\"<label>\" +\n\t\t\t\t        \t\t\t\"<input id=\\\"\"+chckGrpId+\"All\\\"\" +\n                                    \t  \" name=\\\"form[\"+chckGrpId+\"All][]\\\" class=\\\"pmdynaform-control-checkgroup\\\"\" + \n                                          \" value=\\\"chckGrpSelAll\\\" type=\\\"checkbox\\\">\" +\n\t\t\t\t\t        \t\t\"<span><u>\u0395\u03c0\u03b9\u03bb\u03bf\u03b3\u03ae \u03cc\u03bb\u03c9\u03bd<\\\/u><\\\/span>\" +\n\t\t\t\t        \t    \"<\\\/label>\" +\n\t\t\t\t\t\t \"<\\\/div>\" ;\n                                           \n     $(\"#\"+chckGrpId).find(\"div.pmdynaform-checkbox-items\").append(chekboxEl);\n    \n  \t $(\"#\"+chckGrpId+\"All\").click(function () { \/\/ add event for ALL element  \n        $(\"#\"+chckGrpId).find(\"input:checkbox\").not(this).prop('checked', this.checked);\n \t  });\n  \n     drpdnOptions.forEach( function(i, item){\n   \n  \t\t\tchckGrpOptions[item+1] = drpdnOptions[item];\n     \/\/       chckGrpOptions[item+1].['selected'] = false;\n   \n    \t\t chekboxEl = \"<div class=\\\"checkbox\\\">\" +\n            \t\t\t\t\t\"<label>\" +\n\t\t\t\t        \t\t\t\"<input id=\\\"form[\"+chckGrpId+\"][\"+ i['value']+\"]\\\"\" +\n                                    \t  \" name=\\\"form[\"+chckGrpId+\"][]\\\" class=\\\"pmdynaform-control-checkgroup\\\"\" + \n                                          \" value=\\\"\"+i['value']+\"\\\" type=\\\"checkbox\\\">\" +\n\t\t\t\t\t        \t\t\"<span>\"+i['label']+\"<\\\/span>\" +\n\t\t\t\t\t        \t\t\n\t\t\t\t        \t    \"<\\\/label>\" +\n\t\t\t\t\t\t \"<\\\/div>\" ;\n                                           \n            $(\"#\"+chckGrpId).find(\"div.pmdynaform-checkbox-items\").append(chekboxEl);\n    \n  \t\t});\n  \n \t $(\"#\"+chckGrpId).setValue(oldValues);\n    \n      \n  }\n}\n\n\/\/\n\/\/ \n\n\/*\n*\n* \u0394\u0399\u0391\u03a7\u0395\u0399\u03a1\u0399\u03a3\u0397 \u0398\u0395\u039c\u0391\u03a4\u039f\u03a3\n*\n*\/\n\nvar proSubjSelect = ($(\"#reasoning\").getValue().length > 0) ? 1: 0; \/\/\u03b1\u03bd \u03b5\u03c0\u03b9\u03c3\u03c4\u03c1\u03ad\u03c6\u03bf\u03c5\u03bc\u03b5 \u03c3\u03c4\u03b7\u03bd \u03c6\u03bf\u03c1\u03bc\u03b1 \u03ba\u03b1\u03b9 \u03c5\u03c0\u03ac\u03c1\u03c7\u03b5\u03b9 \u03c0\u03c1\u03bf\u03ba. \u0398\u03ad\u03bc\u03b1\nvar reasoningId =   ($(\"#reasoning\").getValue().length > 0 ) ? $(\"#reasoning\").getValue(): -1;\nvar reasoningText;\nvar reasoningDDChange = 0;\n\n$(\"#reasoningInsert\").setValue(0); \/\/\u03b1\u03c1\u03c7\u03b9\u03ba\u03ae \u03c4\u03b9\u03bc\u03ae\n\n$(\"#reasoning\").hide();\n$(\"#reasoningDropDown\").hide();\n\n$(\"#SUBJ\").dblclick(function(){\n\n\t$(\"#reasoning