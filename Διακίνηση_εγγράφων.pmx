        <tri_webbot><![CDATA[// v.3.0 16/3/2017
//

 
function PMFAssociateUploadedFilesWithInputDoc($inputDocumentUid, $gridVariableName, $fileVariableName, $caseUid, $userUid, $currentDelIndex)
{
    try {
        require_once 'classes/model/AppDocument.php';

        $appDocument = new AppDocument();

        //First step: get all the documents from this case and this grid that are not associated
        $criteria = new Criteria('workflow');

        $criteria->add(AppDocumentPeer::APP_UID, $caseUid, Criteria::EQUAL);
        $criteria->add(AppDocumentPeer::APP_DOC_TYPE, 'ATTACHED', Criteria::EQUAL);
       
		$criteria->add(AppDocumentPeer::APP_DOC_FIELDNAME, $gridVariableName . '_%_' . $fileVariableName, Criteria::LIKE);
        $criteria->add(AppDocumentPeer::APP_DOC_STATUS, 'ACTIVE', Criteria::EQUAL);
        $criteria->add(AppDocumentPeer::DOC_UID, '-1', Criteria::EQUAL);
        $criteria->addAscendingOrderByColumn(AppDocumentPeer::APP_DOC_INDEX);

        $numRecTotal = AppDocumentPeer::doCount($criteria);

        //If we have an error in the result set, do not continue
        if ($numRecTotal == 0) {
            //Return
			 
            return false;
        }

        //Query
        $rsCriteria = AppDocumentPeer::doSelectRS($criteria);
        $rsCriteria->setFetchmode(ResultSet::FETCHMODE_ASSOC);
		//
		//
		$inDoc = new InputDocument();
		$oFolder = new AppFolder();
	
		$aIDF = $inDoc->Load($inputDocumentUid);  

		$oFolderUid = $oFolder->createFromPath( $aIDF['INP_DOC_DESTINATION_PATH'],
											   $caseUid );
		
		$aFiles = array();
		$i=1; // array index
		
        //Search file in the process, associate with the Input document
        while ($rsCriteria->next()) {
            $row = $rsCriteria->getRow();
			$file = $appDocument->load($row['APP_DOC_UID']);
 			
			$aFiles[$i]['ver'] = $row['DOC_VERSION'];
			$aFiles[$i]['fileUid'] = $row['APP_DOC_UID'];
			$i++;
			
            //Update
			$file['DOC_UID'] = $inputDocumentUid;
			$file['APP_DOC_TYPE'] = 'INPUT';
            $file['FOLDER_UID'] = $oFolderUid;
            $file['APP_DOC_TAGS'] = $aIDF['INP_DOC_TAGS'];
                 
            $result = $appDocument->update($file);

        }
		 
		return $aFiles;
    } catch (Exception $e) {
        throw $e;
    }
} // end function
//
$kindOfDocs = @@kindOfDocs;
$inDocId = @@inputDocumentUid ; // Input Document UID for current process 

$docFiles =PMFAssociateUploadedFilesWithInputDoc(@@inputDocumentUid, 'gridDocFiles', "fNameD", @@APPLICATION, @@USER_LOGGED, @@INDEX);

$attFiles=PMFAssociateUploadedFilesWithInputDoc(@@inputDocumentUid, 'gridAttFiles', "fNameA", @@APPLICATION, @@USER_LOGGED, @@INDEX);
 

if(!is_array(@=docFiles) ) @=docFiles = array();

$oAppDoc = new AppDocument();

if(is_array($docFiles) and count($docFiles)> 0) {  //έχει φορτωθεί   το έγγραφο και τα πιθανά συνημμένα.  
	
		foreach(@=gridDocFiles as $key => $aFile) {
				 				
				if(!empty($aFile['fCatD'])) {
					$docFiles[$key]['fCat'] =  $aFile['fCatD'];
					$docFiles[$key]['fCat_Str'] = $aFile['fCatD_label'];			
				}
				else {
					$docFiles[$key]['fCat'] =  0;
					$docFiles[$key]['fCat_Str'] = 'Άγνωστo';	
				}
				
				$docFiles[$key]['sign'] =  $aFile['fSignD'];
				$docFiles[$key]['edit'] =  $aFile['fEditD'];
				$docFiles[$key]['diavgeia'] =  $aFile['diavgeia'];
				$docFiles[$key]['prot'] = @@PROT_ID_STR;
				// Ενημέρωση του πίνακα app_document  και του πεδίου 
				
				
				$oContent = $oAppDoc->load($docFiles[$key]['fileUid']);
				
				$oContent['APP_DOC_FIELDNAME'] = "doc:".$docFiles[$key]['fCat_Str'];
				$oContent['APP_DOC_FIELDNAME'] .= ":". $docFiles[$key]['sign'].":" ;
				$oContent['APP_DOC_FIELDNAME'] .= $docFiles[$key]['edit'] . ":" ; 					$oContent['APP_DOC_FIELDNAME'] .= $docFiles[$key]['diavgeia'];
		
				$oAppDoc->update($oContent);
				//
				@@countGridDocFiles = @%countGridDocFiles + 1;
				@@hideGridDocFile = 1;
			
			

		}
		
		@=docFiles[1] = $docFiles[1] ; // υπάρχει μόνο ένα κύριο αρχείο
}
	
// πάντοτε ελέγχουμε για συνημμένα
 
if(	$attFiles !== false ) {
	
		foreach(@=gridAttFiles as $key => $aFile) {
				 				
				if(!empty($aFile['fCatA'])) {
					$attFiles[$key]['fCat'] =  $aFile['fCatA'];
					$attFiles[$key]['fCat_Str'] = $aFile['fCatA_label'];			
				}
				else {
					$attFiles[$key]['fCat'] =  0;
					$attFiles[$key]['fCat_Str'] = 'Άγνωστo';	
				}
				
				

				// Ενημέρωση του πίνακα app_document  και του πεδίου 
				
				
				$oContent = $oAppDoc->load($attFiles[$key]['fileUid']);
				
				$oContent['APP_DOC_FIELDNAME'] = "att:".$attFiles[$key]['fCat_Str'];
				 		
				$oAppDoc->update($oContent);
				//
	
		}
	
		// attach new attached files to array
		$nDocs = count(@=docFiles);
		foreach($attFiles as $key => $aFile){
		
			@=docFiles[$nDocs+$key] = $aFile;
		}
}
 
@%newPanelDocFiles = 1;


]]></tri_webbot>
