        <tri_description><![CDATA[έλεγχος αν αν έχει υπογραφεί το ακριβές αντίγραφο]]></tri_description>
        <pro_uid>723249589575b4a14c53d36097964238</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// v.2.0 23/9/2016
//
$lcSigner = @@USER_LOGGED;

$msg = "";
$reloadForm = false;


 // έλεγχος συστήματος μετά την υπογραφή ακριβούς αντιγράφου

	$oAppDoc = new AppDocument();
	$preVerUser = @@curVerUser; // tracking user's versions
 	

reset(@=docFiles);		
if (count( @=docFiles) > 0) {
	$i = key(@=docFiles);
	$aFile = @=docFiles[$i];
	
	if(@=docFiles[$i]['sign'] == 1 ) { 
		$fileUid = @=docFiles[$i]['fileUid'];
		$lAppDocVer = $oAppDoc->getLastAppDocVersion($fileUid);
			
		if(empty(@=docFiles[$i]['signers'])) {
				
			@=docFiles[$i]['signers'][]= array('userUid' => $lcSigner , 'lFileVer' => $lAppDocVer, 'sDate' => date("d-m-Y H:i:s"));
		}
		else {
			if( @=docFiles[$i]['signers'][0]['userUid'] != $lcSigner ){
				array_unshift(@=docFiles[$i]['signers'], array('userUid' => $lcSigner , 'lFileVer' => $lAppDocVer, 'sDate' => date("d-m-Y H:i:s")));
					
			}
			else {
				@=docFiles[$i]['signers'][0]['lFileVer'] =  $lAppDocVer;
				@=docFiles[$i]['signers'][0]['sDate'] = date("d-m-Y H:i:s");
			}
			
		}
		
		@=docFiles[$i]['lcVer'] = @=docFiles[$i]['signers'][0]['lFileVer'];
		
		@@curVerUser = $lAppDocVer; // track version of current user
		if(empty(@=docFiles[$i]['signers'][1]['lFileVer'])) 
		   $prevSignVer = $aFile['ver'];
		else 
		   $prevSignVer = $aFile['signers'][1]['lFileVer'] ;
		 
		if($prevSignVer >= $lAppDocVer ) {
			$oContent = $oAppDoc->load($fileUid,$lAppDocVer);
			$msg .="Δε έχει αναρτηθεί το Ακριβές Αντίγραφο για το έγγράφο " . $oContent['APP_DOC_FILENAME']. " <br>" ;
			$reloadForm = true; 
			@=legalCopyOK = "";
			@=realLCSign = false;
				
		}
		else {
			if(($lAppDocVer - $preVerUser) >= 1 ) {
				@=realLCSign = true; //recreate panel
				@%newPanelDocFiles = 1;
				$msg = "Νέα έκδοση Ακριβούς Αντιγράφου";
								
			}
			
			// current legal copy version
			
			@=docFiles[$i]['lcVer'] = @=docFiles[$i]['signers'][0]['lFileVer'];	
		}
	} //if sign
		
} //if count
	
	if ($reloadForm)  { //   
		$aData = array('docFiles' => @=docFiles, 
					   'docHasSigned' => @@docHasSigned, 
					   'curVerUser' => @@curVerUser,
					   'legalCopyOK' => @@legalCopyOK,
					   'newPanelDocFiles' => @@newPanelDocFiles,
					   'realLCSign' => @@realLCSign
					  );
		
		PMFSendVariables(@@APPLICATION, $aData);
		
		$dynUID = PMFGetDynaformUID("Ακριβές Αντίγραφο 2.0", @@PROCESS);
		
		G::header("Location: cases_Step?TYPE=DYNAFORM&UID=$dynUID[0]&POSITION=1&ACTION=EDIT");
		
		G::SendMessageText($msg, "INFO");
		die();
		
	}
]]></tri_webbot>
