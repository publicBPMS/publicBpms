var i=1; i<= gRows ; i++ ) {\n  \n    if(oGrid.getValue(i,4).indexOf(\"1\") > -1) isMainRow = i; //set to checked line if exists\n    \n    $(\"#form\\\\[gridListFiles\\\\]\\\\[\"+i+\"\\\\]\\\\[isMain\\\\]\").click({'line':i}, function(evt){\n    \t\n      \tvar r = evt.data.line;\n        \n      if(isMainRow > 0 ) oGrid.setValue(0,isMainRow,4);\n      \n       \n      if(isMainRow == r ) isMainRow = 0;\telse isMainRow = r;\n      \n      console.log('r=',r,' ismain=',isMainRow)\n    \n    })\n  }\n  \n//////////////////////////////////\n\n//====//\n\n}); // window ready\n\n\n\n/*\n* telikos έλεγχος\n*\n*/\n \nvar selForm = getFormById(formId);\n\nselForm.setOnSubmit(function(){\n  \n  var msg = '';\n\n \n  \n});\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title1","label":"Εισερχόμενα: εισαγωγή αρχείων στο BPMS\n","colSpan":12}],[{"type":"multipleFile","variable":"uploaderDocFiles","var_uid":"12043575859629f8fa0adb6024530350","dataType":"multiplefile","protectedValue":false,"id":"uploaderDocFiles","name":"uploaderDocFiles","label":"Επιλογή αρχείων","inputDocument":"Εγγραφα ΠΑΜΘ","required":false,"dnd":false,"extensions":".pdf, .doc, .docx, .xls, .xlsx, .gif, .png, .odt, .ods, .odf","size":24,"sizeUnity":"MB","mode":"parent","multiple":false,"inp_doc_uid":"943782111575b4a18d022d2033249944","var_name":"uploaderDocFiles","colSpan":6},{"type":"button","id":"buttonScan","name":"buttonScan","label":"Σάρωση αρχείων","colSpan":2},{"type":"button","id":"buttonEmail","name":"buttonEmail","label":"e-Mail αρχεία","colSpan":2},{"type":"button","id":"buttonFax","name":"buttonFax","label":"Fax αρχεία","colSpan":2}],[{"type":"panel","id":"panelWebMail","content":"<button type=\"button\" id=\"hideWebMail\" >Απόκρυψη προβολής</button>\n \n  <iframe id=\"ermPortal\" src=\"http://83.212.93.83/webmail/index.php\" width=\"100%\" height=\"650\" frameborder=\"0\" style=\"border:0\">\n\n  </iframe>\n \n<script>      \n  $(\"#panelWebMail\").hide(