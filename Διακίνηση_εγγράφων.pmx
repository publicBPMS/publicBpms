\"background-color\",\"#D8D8C0\");\n$(\"#panelSigners\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelAssign\").css(\"background-color\",\"#D8D8C0\");\n\n\n\n\/\/\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) \n    \t$(selector).show();\n  \telse\n    \t$(selector).hide();\n  }\n}\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelAssign\");\npanelDisplay(\"panelDocFiles\");\npanelDisplay(\"panelSxtFiles\");\npanelDisplay(\"panelInitDocFiles\");\npanelDisplay(\"panelInitRcpts\");\n\/\/\n\/\/\n\n $(\"#gridBpmsRcpts\").hide();\n $(\"#gridOtherRcpts\").hide();\n\n\n\n$(\"#bpmsUsers\").click(function(){\n  $(\"#gridBpmsRcpts\").show();\n  $(\"#gridOtherRcpts\").hide();\n});\n\n$(\"#otherUsers\").click(function(){\n  \n  $(\"#gridBpmsRcpts\").hide();\n  $(\"#gridOtherRcpts\").show();\n});\n\n\n\/* v.2.0 1\/8\/2016\n\/\n\/\u0394\u03b9\u03b1\u03c7\u03b5\u03af\u03c1\u03b9\u03c3\u03b7 \u03c4\u03bf\u03c5 \u03b5\u03be\u03c9\u03c4\u03b5\u03c1\u03b9\u03ba\u03bf\u03cd \u03c0\u03b1\u03c1\u03b1\u03bb\u03ae\u03c0\u03c4\u03b7 - \u0395\u03bd\u03b7\u03bc\u03ad\u03c1\u03c9\u03c3\u03b7 \u03b5\u03c0\u03b9\u03c0\u03bb\u03ad\u03bf\u03bd \u03c3\u03c4\u03bf\u03b9\u03c7\u03b5\u03af\u03c9\u03bd ( CONTACT )\n\/ OTHER GRID setup\n\/\n*\/\nfunction setupRows(){\n\tvar cRows = $(\"#gridOtherRcpts\").getNumberRows();\n\tvar sVal = \"\";\n\n  for (var i = 1; i <= cRows; i++) {\n    var $cDetails = $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+i+\"\\\\]\\\\[cDetails\\\\]\");\n  \t$(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+i+\"\\\\]\\\\[cNameId_label\\\\]\").click(largeName);\n\/\/\n  \tsVal = $(\"#gridOtherRcpts\").getValue(i, 2);\n  \tsVal = sVal.replace(\/\\\\t\/g, \"\\t\");\n  \tsVal = sVal.replace(\/\\\\n\/g, \"\\n\");\n  \t$(\"#gridOtherRcpts\").setValue(sVal, i,2);\n    \n  \t$cDetails.click({ gridRow: i}, function(event) {\n      \t\t\t\t\tvar strSel = \"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[cDetails\\\\]\";\n      \t\t\t\t\tlargeDet(strSel);\n    \t\t\t\t});\n    $cDetails.dblclick({ gridRow: i}, function(event){\n       \t\t\t\t\tvar strSel = \"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[cDetails\\\\]\";\n                        $(\"#gridContSelector\").setValue(strSel); \/\/\u03c7\u03c1\u03b5\u03b9\u03ac\u03b6\u03b5\u03c4\u03b1\u03b9 \u03ad\u03bd\u03b1 hiden field\n       \t\t\t\t\t$( \"#dialog-form\" ).dialog(\"open\");\n      \t\t\t\t\t\n            \t\t\t});\n  }\n} \/\/ setupRows\n \n$(\"form\").setOnchange(setupRows);\nsetupRows(); \/\/on load form\n\n\n\/\/ ON ADDROW\n$(\"#gridOtherRcpts\").onAddRow(function(aNewRow, oGrid, rowIndex) {\n  \n  $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+ rowIndex +\"\\\\]\\\\[cNameId\\\\]\").click(largeName);  \n\/\/\n  var $cDetails = $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+rowIndex+\"\\\\]\\\\[cDetails\\\\]\");\n  setupRows();\n  sVal = aNewRow[1].getValue(); \n  sVal = sVal.replace(\/\\\\t\/g, \"\\t\");\n  sVal = sVal.replace(\/\\\\n\/g, \"\\n\");\n  aNewRow[1].setValue(sVal);\n  $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+ rowIndex +\"\\\\]\\\\[cDetails\\\\]\").click({ gridRow: rowIndex}, function(event) {\n      \t\t\t\t\tvar strSel = \"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[cDetails\\\\]\";\n      \t\t\t\t\tlargeDet(strSel);\n    \t\t\t\t});\n  $(\"#gridOtherRcpts\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n});\n\nfunction largeName(){\n  var cRows = $(\"#gridOtherRcpts\").getNumberRows();\n \n  $(\"#gridOtherRcpts\").hideColumn(3); \n  $(\"#gridOtherRcpts\").hideColumn(4); \n  $(\"#gridOtherRcpts\").hideColumn(5); \n  for (var i = 1; i <= cRows; i++) {\n   $(\"div#\\\\[gridOtherRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[cNameId\\\\]\").parent().parent().css(\"width\", \"50%\");\n  }\n}\n\n\nfunction largeDet(rSel){\n  var cRows = $(\"#gridOtherRcpts\").getNumberRows();\n  $(\"#gridOtherRcpts\").hideColumn(3);\n  $(\"#gridOtherRcpts\").hideColumn(4); \n  $(\"#gridOtherRcpts\").hideColumn(5);\n  $(rSel).attr(\"rows\", \"11\");\n  for (var i = 1; i <= cRows; i++) {\n   $(\"div#\\\\[gridOtherRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[cDetails\\\\]\").parent().parent().css(\"width\", \"50%\");\n  }\n  \n}\n\n\n$(\"#gridOtherRcpts\").focusout(function(){\n   var cRows = $(\"#gridOtherRcpts\").getNumberRows();\n\t$(\"#gridOtherRcpts\").showColumn(3);\n    $(\"#gridOtherRcpts\").showColumn(4);\n     $(\"#gridOtherRcpts\").showColumn(5);\n   for (var i = 1; i <= cRows; i++) {\n     $(\"div#\\\\[gridOtherRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[cNameId\\\\]\").parent().parent().css(\"width\", \"30%\");\n     $(\"div#\\\\[gridOtherRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[cDetails\\\\]\").parent().parent().css(\"width\", \"30%\");\n     $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\" +  i+ \"\\\\]\\\\[cDetails\\\\]\").attr(\"rows\", \"1\");\n   }\n                      \n  });\n\n\/\/ gridBpmsRcpts\n\/\/ ON ADDROW\n$(\"#gridBpmsRcpts\").onAddRow(function(aNewRow, oGrid, rowIndex) {\n  \n    $(\"#gridBpmsRcpts\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n});\n\/\/\n\/\/\n\/\/\n\n  if($(\"#bpmsRcptsJSON\").getValue().length != 0 ) {\n\tvar bpmsRcpts = JSON.parse($(\"#bpmsRcptsJSON\").getValue());\n\t\n\tvar r = $(\"#gridBpmsRcpts\").getNumberRows();\n\tfor (var i = 1 ; i <= r ; i++) {\n  \t\tif (typeof bpmsRcpts[i] != 'undefined' && bpmsRcpts[i]['rcptDep'] != '') {\n    \t$(\"#gridBpmsRcpts\").setValue(bpmsRcpts[i]['rcptDep'], i, 1);\n  \t\t\n\t\t} \n  \t}\n  }\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title1","label":"\u03a0\u03b1\u03c1\u03b1\u03bb\u03ae\u03c0\u03c4\u03b5\u03c2 \u0394\u03b9\u03b1\u03b2\u03b9\u03b2\u03b1\u03b6\u03cc\u03bc\u03b5\u03bd\u03bf\u03c5 \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03bf\u03c5","colSpan":12}],[{"typ