		if($fCat == "gridDocFiles")  $typCat = "doc:".$dClass[$i-1].":".$signEdit[$i-1].":".$diavgeia[$i-1]; 
		if($fCat == "gridAttFiles")  $typCat = "att:".$dClass[$i-1].":::";
		 
// APP_DOC_FIELDNAME = 'doc' or 'attached' χαρακτηρίζει το κανονικό έγγραφο ( που υπογράφεται) και τα συνημμένα. 
		$query = "UPDATE APP_DOCUMENT SET 
		DOC_UID = '$inDocId', 
		APP_DOC_TYPE ='INPUT',
		APP_DOC_INDEX = '$newIndex' ,
		FOLDER_UID ='$oFolderUid',
		APP_DOC_TAGS ='$tags' ,
		APP_DOC_FIELDNAME = '$typCat'
		WHERE APP_DOC_UID = '$docUid'";
