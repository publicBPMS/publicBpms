				@@onceAda = 0;
	
			}
			break;

	case 0:
	
			if(strlen(@@ADA) == 14 ){
			
				$nfileUid = @=docFiles[1]['fileUid'];
				
				$oAppDoc = new AppDocument();
						
				$oContent = $oAppDoc->load($nfileUid); // load new version with ADA
						
				$oContent['APP_DOC_FIELDNAME'] = "doc:".@@DOC_CLASS_STR.":0:0:0";
		 							
				$oAppDoc->update($oContent);
				@=docFiles[1]['fCat'] = @@DOC_CLASS;			
				@=docFiles[1]['fCat_Str'] = @@DOC_CLASS_STR;			
				@=docFiles[1]['ver'] = $oContent['DOC_VERSION'];			
				@=docFiles[1]['adaVer'] = $oContent['DOC_VERSION'];
			}
			break;
		

	case -1:
				
