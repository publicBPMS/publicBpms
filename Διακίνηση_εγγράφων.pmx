        <tri_webbot><![CDATA[//v.3.1 28/2/2017

$actionDate = date("Y-m-d H:i:s");
$protDateGR = formatDate($actionDate, 'dd-mm-yyyy h:i:s');

$ADA  = @@ADA ;
$ADA_DATE  = @@ADA_DATE ;
$CANCELLED  = @@CANCELLED ;
$CLOSE_DATE  = $actionDate ;
@@CLOSE_DATE  = $actionDate ;
$CLOSER  = @@USER_LOGGED ;
@@CLOSER  = @@USER_LOGGED ;
$COMMENTS  = @@COMMENTS ;
$DEP_UID  = @@DEP_UID;
$DEP_MAN = @@DEP_MAN;
 
//$resp = executeQuery("SELECT DEP_MANAGER FROM DEPARTMENT  WHERE DEP_UID = '$DEP_UID'");
//if(is_array($resp) and count($resp) > 0) $DEP_MAN = $resp[1]['DEP_MANAGER'];
//@@DEP_MAN = $DEP_MAN;


$DOC_CLASS  = @@DOC_CLASS ;
$DOC_DATE  = $actionDate ;
@@DOC_DATE  = $actionDate ;
$DOC_DLVRY = @@DOC_DLVRY ;
$DOC_ID  = @@DOC_ID ;
$DOC_TYPE  = @@DOC_TYPE ;
$DOC_YEAR  = formatDate($actionDate, 'yyyy'); 
@@DOC_YEAR  = $DOC_YEAR; 
$EDITOR  = @@EDITOR ;
$EDITOR_DEP = @@EDITOR_DEP;
$FILE_CAB  = @@FILE_CAB ;
$GRADING  = @@GRADING ;
$OPENER  = @@OPENER ;
$OPENER_DEP = @@OPENER_DEP;
$OTHER  = @@OTHER ;
$P_CASE  = @@P_CASE ;
$PRIORITY  = @@PRIORITY ;
$PROT_DATE  = $actionDate ;
@@PROT_DATE  = $actionDate ;
$PROT_ID  = @%PROT_ID ;
$PROT_ID_DEP  = @%PROT_ID_DEP ;
$PROT_REG_DATE  = $actionDate ;
@@PROT_REG_DATE  = $actionDate ;
$RCPT_DATE  = '' ;
$SCOPE  = @@SCOPE ;
$SEND_DATE  = $actionDate ;
@@SEND_DATE  = $actionDate ;
$SENDER  = @@SENDER ;
$SIGN_CASE  = @@SIGN_CASE ;
$STATUS  = 2 ; // κλειστό
@@STATUS  = 2 ; // κλειστό
@@STATUS_STR  = "Kλειστό" ; // κλειστό
$SUBJ  = @@SUBJ ;
$SUBJ_THEME  = @@SUBJ_THEME ;
$SXETIKA  = @@SXETIKA ;
$YEAR  = @@YEAR ; //προσοχή στην περίπτωση εξερχομένου , περιλαμβάνει το έτος εισερχ.


//Πρωτοκόλληση 

//
if(@@DOC_TYPE == 4 ) { // Εξερχόμενο - Απάντηση σε εισερχόμενο
	
	 
	
		
	@@DOC_ID = @%PROT_ID."/".@%PROT_ID_DEP.' '. $protDateGR;
	@@PROT_ID_STR = @@DOC_ID;
	
	$DOC_ID = @@DOC_ID;
	@@CLOSE_RSN = "Ενέργεια εισερχομένου";
	$CLOSE_RSN = @@CLOSE_RSN;
	// προσοχή, η τιμή του πεδίου YEAR λαμβάνεται απο το $DOC_YEAR. Η μεταβλητή @@YEAR
	// έχει τιμή το έτος του εισερχομένου. Χρειάζεται στο παρακάτω update και στην
	//φόρμα σε περίπτωση επαναφόρτωσης.
	
		$qIns = "INSERT INTO PMT_PROTOKOLLO_PAMTH ( ADA, ADA_DATE,CLOSE_DATE,CLOSE_RSN, CLOSER, COMMENTS, DEP_MAN, DEP_UID, DOC_CLASS, DOC_DATE, DOC_DLVRY, DOC_ID, DOC_TYPE, DOC_YEAR, EDITOR, EDITOR_DEP, FILE_CAB, GRADING, OPENER, OPENER_DEP, OTHER, P_CASE, PRIORITY, PROT_DATE, PROT_ID, PROT_ID_DEP, PROT_REG_DATE, RCPT_DATE,  SCOPE, SEND_DATE,  SENDER, SIGN_CASE, STATUS, SUBJ, SUBJ_THEME, SXETIKA, YEAR ) 
VALUES ( '$ADA', '$ADA_DATE', '$CLOSE_DATE','$CLOSE_RSN','$CLOSER','$COMMENTS',  '$DEP_MAN', '$DEP_UID','$DOC_CLASS', '$DOC_DATE', '$DOC_DLVRY','$DOC_ID', '$DOC_TYPE', '$DOC_YEAR', '$EDITOR','$EDITOR_DEP', '$FILE_CAB', '$GRADING', '$OPENER','$OPENER_DEP', '$OTHER', '$P_CASE', '$PRIORITY', '$PROT_DATE', '$PROT_ID', '$PROT_ID_DEP', '$PROT_REG_DATE', '$RCPT_DATE',  '$SCOPE', '$SEND_DATE', '$SENDER', '$SIGN_CASE', '$STATUS', '$SUBJ', '$SUBJ_THEME', '$SXETIKA', '$DOC_YEAR' )"; //$DOC_YEAR: δες πιο πάνω 
	$res = executeQuery($qIns);
	
	executeQuery("UPDATE PMT_PROTOKOLLO_PAMTH 
				  SET STATUS = '$STATUS',
							CLOSER = '$CLOSER',
							CLOSE_DATE = '$CLOSE_DATE',
							CLOSE_RSN = '$CLOSE_RSN', 
							COMMENTS = '$COMMENTS',
							OTHER = '$OTHER' 
					WHERE  YEAR = '$YEAR' AND PROT_ID = '$PROT_ID' AND DOC_TYPE = 1 "); //close incom
										
} // if @@DOC_TYPE == 4
else { // οίκοθεν @@DOC_TYPE = 2;
	
		
	@@YEAR = @@DOC_YEAR;
	$YEAR = @@YEAR;
	@@CLOSE_RSN = "Οίκοθεν";
	$CLOSE_RSN = @@CLOSE_RSN;

//γενικό πρωτόκολλο
	$query = "SELECT IFNULL(MAX(PROT_ID),0)+1 as MG
   		      FROM PMT_PROTOKOLLO_PAMTH
			  WHERE YEAR = '$YEAR' ";
	$maxG = executeQuery($query);
	$PROT_ID = $maxG[1]['MG'];
	@@PROT_ID = $PROT_ID;
//
//ειδικό πρωτόκολλο
	$query = "SELECT IFNULL(MAX(PROT_ID_DEP),0)+1 as MD
   		      FROM PMT_PROTOKOLLO_PAMTH
			  WHERE YEAR = '$YEAR' and DEP_UID = '$DEP_UID'";
	$maxD = executeQuery($query);
	$PROT_ID_DEP = $maxD[1]['MD'];
	@@PROT_ID_DEP = $PROT_ID_DEP;
//
	
//ενημέρωση πρωτοκόλλου
	$qIns = "INSERT INTO PMT_PROTOKOLLO_PAMTH ( ADA, ADA_DATE,CLOSE_DATE,CLOSE_RSN, CLOSER, COMMENTS, DEP_MAN, DEP_UID, DOC_CLASS, DOC_DATE, DOC_DLVRY, DOC_ID, DOC_TYPE, DOC_YEAR, EDITOR, EDITOR_DEP, FILE_CAB, GRADING, OPENER, OPENER_DEP, OTHER, P_CASE, PRIORITY, PROT_DATE, PROT_ID, PROT_ID_DEP, PROT_REG_DATE, RCPT_DATE,  SCOPE, SEND_DATE,  SENDER, SIGN_CASE, STATUS, SUBJ, SUBJ_THEME, SXETIKA, YEAR ) 
VALUES ( '$ADA', '$ADA_DATE', '$CLOSE_DATE','$CLOSE_RSN','$CLOSER','$COMMENTS',  '$DEP_MAN', '$DEP_UID','$DOC_CLASS', '$DOC_DATE', '$DOC_DLVRY','$DOC_ID', '$DOC_TYPE', '$DOC_YEAR', '$EDITOR','$EDITOR_DEP', '$FILE_CAB', '$GRADING', '$OPENER','$OPENER_DEP', '$OTHER', '$P_CASE', '$PRIORITY', '$PROT_DATE', '$PROT_ID', '$PROT_ID_DEP', '$PROT_REG_DATE', '$RCPT_DATE',  '$SCOPE', '$SEND_DATE', '$SENDER', '$SIGN_CASE', '$STATUS', '$SUBJ', '$SUBJ_THEME', '$SXETIKA', '$YEAR' )";
	
	$res = executeQuery($qIns);
	
	if($res == 0 ) {
		G::SendMessageText("Δεν έγινε ενημέρωση πρωτοκόλλου", 'WARNING' );
//		G::header("location: cases_List");
//		die();
	}//if $res
	
	$PROT_ID_STR = $PROT_ID ."/". $PROT_ID_DEP ." ". $protDateGR ;
	@@PROT_ID_STR = $PROT_ID_STR;
	
}//else doc type

// σε κάθε περίπτωση ενημερώνεται ο πίνακας των εξωτερικών παραληπτών
if(count(@=rcpts)> 0){
	foreach(@=rcpts as $grp => $grpArray) {
		foreach ($grpArray as $subGrp => $rcptsArray ) {
