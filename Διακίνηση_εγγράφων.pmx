e {\n        return true;\n      }\n    }\n \n    function addDetails() {\n      var valid = true;\n      allFields.removeClass( \"ui-state-error\" );\n \n      valid = valid && (contPerson.val().length >0) ? checkLength( contPerson, \"\u03c4\u03bf\u03c5 \u03bf\u03bd\u03cc\u03bc\u03b1\u03c4\u03bf\u03c2\", 6, 32 ): true;\n      valid = valid && (contEmail.val().length >0) ? checkLength( contEmail, \"email\", 6, 80 ): true;\n \n      valid = valid && (contEmail.val().length >0) ? checkRegexp( contEmail, emailRegex, \"\u03a3\u03c6\u03ac\u03bb\u03bc\u03b1 \u03c3\u03c4\u03bf e-mail\" ): true;\n     \n \n      if ( valid ) {\n        $calSelector.val( \"\u0395\u03c0\u03b1\u03c6\u03ae\\t:\\t\"+ contPerson.val()+\"\\n\"+\n        \t\t\"\u039f\u03b4\u03cc\u03c2\\t:\\t\"+contAddress.val()+ \"\\n\"+\n               \"\u03a4.\u0398\\t\\t:\\t\"+ contPOBox.val()+ \"\\n\"+\n               \"\u03a0\u03b5\u03c1\u03b9\u03bf\u03c7\u03ae:\\t\"+ contArea.val()+\"\\n\"+\n               \"\u03a4.\u039a\\t\\t:\\t\"+ contZip.val()+ \"\\n\"+\n               \"\u0394\/\u03bd\u03c3\u03b7\\t:\\t\"+contDep1.val()+ \"\\n\"+\n               \"\u03a4\u03bc\u03ae\u03bc\u03b1\\t:\\t\"+ contDep2.val()+ \"\\n\"+\n                \"E-Mail\\t:\\t\"+contEmail.val()+ \"\\n\"+\n               \"\u03a4\u03b7\u03bb.\\t:\\t\"+ contPhone.val()+ \"\\n\"+\n               \"Fax\\t\\t\\:\\t\"+ contFax.val()+ \"\\n\"+\n               \"\u0386\u03bb\u03bb\u03b1\\t:\\t\"+contDesc.val()+ \"\\n\" );\n        \n       dialog.dialog( \"close\" );\n      }\n      return valid;\n    }\n \/\/  \n  \tfunction setDialogForm() {\n\n       $calSelector = $($(\"#gridContSelector\").getValue());\n  \t\tvar elmnts = [];\n      \tvar eDetails = $calSelector.val();\n      \tvar cLines = eDetails.split(\"\\n\");\n        cLines.pop(); \/\/\u03b7 \u03c4\u03b5\u03bb\u03b5\u03c5\u03c4\u03b1\u03af\u03b1 \u03b3\u03c1\u03b1\u03bc\u03bc\u03b1\u03ae \u03b5\u03af\u03bd\u03b1\u03b9 \u03ba\u03b5\u03bd\u03ae\n      \tcLines.forEach(function(item, indx ) {\n  \t\t\tvar elmnt;\n          \titem.trim();\n            elmnt= item.split(\":\");\n    \t\t$(allFields[indx]).val( elmnt[1].trim());\n                    \n  \t\t});\n  \t\t\n\t}\n\/\/\n \n   dialog = $( \"#dialog-form\" ).dialog({\n      autoOpen: false,\n      height: 600,\n      width: 500,\n      modal: true,\n      buttons: {\n        \"\u0391\u03c0\u03bf\u03b8\u03ae\u03ba\u03b5\u03c5\u03c3\u03b7\": addDetails,\n         \"\u0391\u03ba\u03cd\u03c1\u03c9\u03c3\u03b7\": function() {\n          dialog.dialog( \"close\" );\n        }\n      },\n      open: setDialogForm,\n      close: function() {\n        form[ 0 ].reset();\n        allFields.removeClass( \"ui-state-error\" );\n      }\n    });\n \n    form = dialog.find( \"form\" ).on( \"submit\", function( event ) {\n      event.preventDefault();\n      addDetails();\n    });\n \n    \n  })); \/\/ ducument\n    \n  <\/script>\n<\/div>\n \n\n<\/body>\n<\/html>","border":"1px","colSpan":12}],[{"type":"hidden","variable":"gridContSelector","var_uid":"39459228557b275e4ece6a0066054536","dataType":"string","protectedValue":false,"id":"gridContSelector","name":"gridContSelector","defaultValue":"","datasource":"database","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","dataVariable":"","var_name":"gridContSelector","colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1},{"colSpan":1}]],"variables":[{"var_uid":"39459228557b275e4ece6a0066054536","prj_uid":"723249589575b4a14c53d36097964238","var_name":"gridContSelector","var_field_type":"string","var_field_size":10,"var_label":"string","var_dbconnection":"workflow","var_dbconnection_label":"PM Database","var_sql":"","var_null":0,"var_default":"","var_accepted_values":[],"inp_doc_uid":"","var_uid_old":"39459228557b275e4ece6a0066054536","var_name_old":"gridContSelector","prj_uid_old":"723249589575b4a14c53d36097964238","create":false}]}]}]]></dyn_