	$SUBJ = @@SUBJ .' '. $anAuthRow['cNameId_label'];
	$P_NAME = $anAuthRow['cNameId_label'];
	
	if(@%countRcpts >= 1 ) { //or @%countRRcpts >= 1 ){  // scope rcpts and Real Rcpts
	
	$aRcpts = @=rcpts['bpms'][@@protScopeGroupUid];
	
	foreach($aRcpts as $aMan => $aManProp) {
