					@=docFiles[1]['fCat_Str'] = @@DOC_CLASS_STR;
					@=docFiles[1]['ver'] = $oContent['DOC_VERSION'];
					@=docFiles[1]['adaVer'] = $oContent['DOC_VERSION'];
			
			
