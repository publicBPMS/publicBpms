					"&usrid=".$user.
					"&duid=".$inDocId.
					"&dtyp=INPUT" ;
	 
	
	 	$view = "<a title=\"προβολή τελευταίας έκδοσης \"".
				"class = \"".$embedPdf."\" ".
				$viewHref.
				"&fname=".$encfileName.
				"\"> <img src=\"/images/documents/_view.png\" ></a>";
	
	}//if P_CASE_REL exists
		
	if($fileCat == "doc") { //κύριο έγγραφο
	$paneltd .= "<tr>";
	$paneltd .= "<td>".$view."  </td>";
	$paneltd .= "<td>".$fileNameUrl ."</td>";
	$paneltd .= "<td id=prot> ".$aFile['PROT_ID']."/".$aFile['YEAR']."</td>";	 $paneltd .= "<td>" .$docType  . "</td>";
	
	$paneltd .= "<td>" .$aFile['SUBJ']  . "</td>";
	
	$paneltd .= "<tr>";
		}
			else { // συνημμένα
	$panelta .= "<tr>";
	$panelta .= "<td>".$view." </td>";
	$panelta .= "<td>".$fileNameUrl ."</td>";
	$panelta .= "<td> </td>";
	
	$panelta .= "<td>" .$docType  . "</td>";
	$paneltd .= "<td>" .$aFile['SUBJ']  . "</td>";
	$panelta .= "<tr>";
			}
				
	
} // for
	//
