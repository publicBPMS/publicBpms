else {$cExistInpDoc = 0;}



//move ATTACHED files to InputDocument
// first merge comments of uploaded files


$query = "SELECT APP_DOC_UID as appDocUid, APP_DOC_FIELDNAME as  gridField
			FROM APP_DOCUMENT
			WHERE  	 APP_UID = '$caseId'
			and DOC_UID = '-1'";
$res = executeQuery($query);
