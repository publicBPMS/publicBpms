        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[//v 3.0 23/3/2017 
//
$caseId = @@APPLICATION;
$lang=@@SYS_LANG;
$index = @@INDEX;
$user = @@USER_LOGGED;



$inDocId = @@inputDocumentUid;


if(is_array(@=docFiles))  $cResp = count(@=docFiles) ; else $cResp = 0 ; 

if($cResp > 0 ){
	$oAppDoc = new AppDocument();
	@@newDocFiles = 0;  // until to add new files

  foreach (@=docFiles as $i => $aFile) { 
	if(!empty($aFile['fileUid'])){
		$fileUid = $aFile['fileUid'];
		$lAppDocVer = $oAppDoc->getLastAppDocVersion($fileUid,$caseId);
		$oContent = $oAppDoc->load($fileUid,$lAppDocVer);
	
		if( $lAppDocVer > 1 ) { 
			$aAppDoc = new AppDocument();
			$aContent = $aAppDoc->load($fileUid,1);
			$oContent['APP_DOC_COMMENT']= $aContent['APP_DOC_COMMENT'];
			$oContent['APP_DOC_TITLE'] = $aContent['APP_DOC_TITLE'];
			$oContent['APP_DOC_FIELDNAME'] = $aContent['APP_DOC_FIELDNAME'];
		
			$oAppDoc->update($oContent);
		}
	
		$fextention = pathinfo($oContent['APP_DOC_FILENAME'], PATHINFO_EXTENSION);
	
		$fileName  = $oContent['APP_DOC_FILENAME'];
		$createDate = $oAppDoc->getAppDocCreateDate('d-m-Y H:i:s');
		$fileIndex  = 	$oAppDoc->getAppDocIndex();
		$urlFile = 'http://' . $_SERVER['HTTP_HOST'] . '/sys' . @@SYS_SYS . '/' . @@SYS_LANG . '/' . @@SYS_SKIN . '/cases/cases_ShowDocument?a=' . $fileUid;
		$filePath = PATH_DOCUMENT . G::getPathFromUID($caseId) . PATH_SEP . $fileUid. '_'.$lAppDocVer . '.' . $fextention ;
		$docExtImg = "/images/documents/extension/" . $fextention . ".". "png";
		list($fileCat, $docClass, $sign, $edit, $diavgeia) = explode(":", $oContent['APP_DOC_FIELDNAME']) ;
		@@diavgeia = $diavgeia;
	
		$wwwHost = 'http://' . $_SERVER['HTTP_HOST'];
	 
	
//
//προετοιμασία panel
	
	
		$encfileName = urlencode(mb_convert_encoding($fileName, "ISO-8859-7" ,"UTF-8"));
	
		$view = "";
		$editUrl = "";
		$signUrl = "";
		$embedPdf = "";
		$signImg = "";
		
		(@@selVersion) ? $ver = "&ver=". @@selVersion : $ver = "&ver=". $lAppDocVer;
		
		$urlfile = 'http://' . $_SERVER['HTTP_HOST'] . '/sys' . @@SYS_SYS . '/' . @@SYS_LANG . '/' . @@SYS_SKIN . '/cases/cases_ShowDocument';	
		 
		if($fextention == "pdf") {
			$embedPdf = "embed-pdf";
			$viewHref = "href=\"".$urlFile.$ver."&ext=pdf" ;
		} 
		else {
		
			$viewHref = "href=\"http://localhost:34300/view?urlFile=".$urlfile.$ver.
					"&ext=".$fextention.
					"&fuid=". $fileUid ;
			$embedPdf = "";
		
		}
	
	 
		$urlParams ="?urlFile=".  $urlfile.
					"&fuid=".$fileUid.
					"&fname=".$encfileName.
					"&indx=". $fileIndex.
					"&ver=".$lAppDocVer. 
					"&caseid=".$caseId.
					"&usrid=".$user.
					"&duid=".$inDocId.
					"&dtyp=INPUT".
					"&JSIGNPDF=".@=jspSel ;
	 
	
	 	$view = "<a  title=\"προβολή τελευταίας έκδοσης\" ".
				"class = \"".$embedPdf."\" ".
				$viewHref.
				"&fname=".$encfileName.
				"\"> <img src=\"" . $docExtImg . "\" alt=\"Προς υπογραφή\"></a>";
	
	
		if($sign == 1) {
		
			$signUrl = "<a class=\"signDocument\" title=\"Υπογραφή\" ".
				"href=\"http://localhost:34300/sign".$urlParams .
				"\"> <img src=\"/images/ext/sign.png\"></a>";
			$signImg = "<img src=\"/images/ext/sign.png\">";
		}
		
	
		if($edit == 1) {
		
			$editUrl = "<a class=\"editDocument\" title=\"Επεξεργασία εγγράφου\" ".
			"href=\"http://localhost:34300/edit". $urlParams .
			"\"> <img src=\"/images/ext/edit.png\"></a>";
		}
	
	
	
		if($fileCat == "doc") { //κύριο έγγραφο
			$paneltd .= "<tr>";
			$paneltd .= "<td>".$view.$editUrl."  </td>";
			$paneltd .= " <td> <a href=\"" . $urlFile . "\">" .  $fileName . "</a></td>";
			
//			$paneltd .= "<td>" .$lAppDocVer . "</td>";
			$paneltd .= "<td>" .$docClass  . "</td>";
			$paneltd .= "<td>" . $createDate  . "</td>";
			$paneltd .= "<tr>";
		}
		
		else { // συνημμένα
			$panelta .= "<tr>";
			$panelta .= "<td>".$view." </td>";
			$panelta .= " <td> <a href=\"" . $urlFile . "\">" .  $fileName . "</a></td>";
			
//			$panelta .= "<td>" .$lAppDocVer. "</td>";
			$panelta .= "<td>" .$docClass   . "</td>";
			$panelta .= "<td>" . $createDate  . "</td>";
			$panelta .= "<tr>";
		}
				
		@=docFiles[$i]['ver'] = $lAppDocVer;
	 } // if
	
   } // for
	
// make  panel for εξερχόμενα
	if(isset($paneltd) and strlen($paneltd) > 10 ) {
		@@panelDocFiles = "<b><p>". @@kindOfDocs ." έγγραφo:</p> </b>
		<table style=\"width:100%\">
 			<tr>
   				<th>Ενέργειες </th>
    			<th>Όνομα Αρχείου</th>
    			<th>Κατ. Εγγράφου</th> 
				<th>Ημ/νία Εισαγωγής</th>
   			</tr> ";

	if(isset($panelta) and strlen($panelta) > 10 ) 
		$panelta = "<tr><td><br><p><b>Συνημμένα</b></p><td><tr>". $panelta;

	@@panelDocFiles .= $paneltd . $panelta. "</table>";	


@@panelDocFiles .= "
<style>
.pdfobject-container { height: 500px;}
.pdfobject { border: 1px solid #666; }
</style>
<script src=\"$wwwHost/pdfobject/pdfobject.min.js\"> </script>

<button type=\"button\" id=\"hidePdf\" >Απόκρυψη προβολής</button>
<div id='viewPdf'></div>

<script>

var \$node = $(\"#viewPdf\");

var clickHandler = function (e){

	e.preventDefault();

	var pdfURL = this.getAttribute(\"href\");
  
	var options = {
	pdfOpenParams: {
		navpanes: 0,
		toolbar: 0,
		statusbar: 0,
		view: \"FitV\"
		}, 
    forcePDFJS: true,
	PDFJS_URL:  \"$wwwHost/pdfjs/web/viewer.html\"
    };
 
  $(\"#viewPdf\").show();
  $(\"#hidePdf\").show();
  
  var myPDF = PDFObject.embed(pdfURL, \$node, options);
	
  $(\"#hidePdf\").click(function(){
	    	$(\"#viewPdf\").hide();
			$(\"#hidePdf\").hide();
			
		});
};

var a = document.querySelectorAll(\".embed-pdf\");

for(var i=0; i < a.length; i++){
  
  	a[i].addEventListener('click', clickHandler);
}

$(\"#hidePdf\").hide()
</script>"	;
	} // panel was made 
} // - if at begining]]></tri_webbot>
