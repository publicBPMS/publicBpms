			FROM PMT_PROTOKOLLO_PAMTH   
			LEFT JOIN PMT_AUTHORITIES  ON  SENDER =  A_ID 
			WHERE  $whereClause
			UNION 
			 SELECT P_CASE,YEAR,PROT_ID,DOC_TYPE,A_NAME, DOC_ID,SUBJ 
			FROM  PMT_PROTOKOLLO_PAMTH   
			LEFT JOIN PMT_SXETIKA ON  YEAR = SX_YEAR_REL AND  PROT_ID = SX_PROT_ID_REL
			LEFT JOIN PMT_AUTHORITIES ON  SENDER =  A_ID
			WHERE $sxetWhereClause ) S
			LEFT JOIN APP_DOCUMENT  ON P_CASE =  APP_UID 
			WHERE IF(APP_UID != '', APP_DOC_FIELDNAME like 'doc%', 1) 
			ORDER BY YEAR, PROT_ID, DOC_TYPE";
			

		
		$sxtFiles = executeQuery($qry);
  
		} //else docHasReg
 
if(is_array($sxtFiles) and count($sxtFiles)>0  ){
	$oAppDoc = new AppDocument();
	
foreach ($sxtFiles as $i => $aFile) { 
	 
	$view = "";
	$editUrl = "";
