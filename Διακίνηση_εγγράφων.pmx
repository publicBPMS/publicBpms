        <dyn_content><![CDATA[{"name":"\u0391\u03bd\u03ac\u03c1\u03c4\u03b7\u03c3\u03b7 \u03b5\u03be\u03b5\u03c1\u03c7\u03bf\u03bc\u03ad\u03bd\u03c9\u03bd \u03b3\u03b9\u03b1 \u03c5\u03c0\u03bf\u03b3\u03c1\u03b1\u03c6\u03ae 2.1","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"68460083057ac69c4eff311003303345","name":"\u0391\u03bd\u03ac\u03c1\u03c4\u03b7\u03c3\u03b7 \u03b5\u03be\u03b5\u03c1\u03c7\u03bf\u03bc\u03ad\u03bd\u03c9\u03bd \u03b3\u03b9\u03b1 \u03c5\u03c0\u03bf\u03b3\u03c1\u03b1\u03c6\u03ae 2.1","description":"","mode":"edit","script":{"type":"js","code":"$(\"form\").css(\"background-color\", \"#DEE0EC\");\n\n$(\".form-control\").css(\"font-size\", \"0.9em\");\n$(\".textlabel\").css(\"font-size\", \"0.88em\");\n$(\".title-column\").css(\"font-size\", \"0.88em\");\n$(\".pmdynaform-grid-title\").css(\"font-size\", \"0.9em\");\n\n$(\"#form\\\\[toSignSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[rcptsSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#sxtTitle\").css(\"color\",\"#990000\");\n\n$(\"#title1\").css(\"color\",\"#ffffff\");\n$(\"#title1\").css(\"background-color\",\"#428bca\");\ndocument.getElementById('title1').style.borderRadius= \"8px\";\n\n\ndocument.getElementById(\"gridDocFiles\").style.padding = \"5px 10px 20px 10px\";\ndocument.getElementById('gridDocFiles').style.borderRadius= \"10px\";\ndocument.getElementById(\"gridAttFiles\").style.padding = \"5px 10px 20px 10px\";\ndocument.getElementById('gridAttFiles').style.borderRadius= \"10px\";\n\n$(\"#reasoning\").css(\"background-color\",\"#F8F00A\");\n$(\"#reasoningDropDown\").css(\"background-color\",\"#F8F00A\");\n\n$(\"#gridDocFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#gridDocFiles-body\").children().children().css(\"background-color\",\"#CCD8CC\");\n$(\"#gridAttFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#gridAttFiles-body\").children().children().css(\"background-color\",\"#CCD8CC\");\n$(\"#panelSxtFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelDocFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelIncDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n\n$(\"#panelIncRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelSigners\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelAssign\").css(\"background-color\",\"#D8D8C0\");\n\n$(\"#gridAssignTo\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridAssignCc\").css(\"background-color\",\"#D8D8C0\");\n\/\/\n\/\/\n\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) {\n    \t$(selector).show();\n      if( elId.indexOf(\"SxtFiles\") > 0 || elId.indexOf(\"IncDocFiles\") > 0 ) {\n        $(\"#sxetikaButton\").show();\n        $(selector).hide();\n      }\n    }\n    else {\n    \t$(selector).hide();\n      if( elId.indexOf(\"SxtFiles\") > 0 && elId.indexOf(\"IncDocFiles\") > 0 ) $(\"#sxetikaButton\").hide();\n    }\n  }\n}\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelAssign\");\npanelDisplay(\"panelDocFiles\");\npanelDisplay(\"panelSxtFiles\");\npanelDisplay(\"panelIncDocFiles\");\npanelDisplay(\"panelIncRcpts\");\n\n\/\/ - panel\n\n   \n   $(\"#panelIncRcpts\").hide();\n   $(\"#panelIncDocFiles\").hide();\n   $(\"#panelSxtFiles\").hide();\n  \n\/\/\n\/\/\n \n\n$(\".uploadNewVersion\").click(function(){\n  \t$(\"#dynaformPos\").setValue(2); \/\/ \u03b3\u03b9\u03b1 \u03c5\u03c0\u03bf\u03bb\u03bf\u03b3\u03b9\u03c3\u03bc\u03cc \u03c4\u03b7\u03c2 \u03c6\u03cc\u03c1\u03bc\u03b1\u03c2 \u03c3\u03c3\u03c4\u03bf \u03b2\u03ae\u03bc\u03b1 2 \u03c4\u03bf\u03c5 task\n    $(\"#uploadInputDocument\").setValue(1); \/\/allow to run inputdocument form\n\tselForm.saveForm();\n\n});\n\n\/\/\n\/\/\n$(\"a.signDocument\").remove();\n\/\/$(\"a.editDocument\").remove();\n$(\"a.editLCDocument\").remove();\n$(\"a.validDocument\").remove();\n\n$(\"a.signIncDocument\").remove();\n$(\"a.editIncLCDocument\").remove();\n$(\"a.validIncDocument\").remove();\n$(\"a.uploadIncNewVersion\").remove();\n \n\/\/\n \n$(\"#sxetikaButton\").click(function(){\n    $(\"#panelIncRcpts\").toggle();\n    $(\"#panelIncDocFiles\").toggle();\n    $(\"#panelSxtFiles\").toggle();\n    \n});\n\n$(\"#rcptsSubmit\").click(function(){$(\"#showRcptsForm\").setValue(1);});\n$(\"#toSignSubmit\").click(function(){$(\"#showRcptsForm\").setValue(0);});\n\/\/\n$(\"#gridAttFiles\").hide();\n$(\"#synimmenaButton\").click(function(){$(\"#gridAttFiles\").toggle();});\n\n\n\/\/\n\n\n\n  \/\/ hide grid doc files \n  if ( $(\"#hideGridDocFile\").getValue() == 1) {\n  \n  \t $(\"#gridDocFiles\").hide();\n     $(\"#gridDocFiles\").disableValidation(1);\n     $(\"#gridDocFiles\").disableValidation(2);\t\n  \t\t\n  }\n\n\n\/\/ $(\"#sxtFilesButton\").css(\"align-items\", \"left\");\n\n\/*\n*\n* \u0394\u0399\u0391\u03a7\u0395\u0399\u03a1\u0399\u03a3\u0397 \u0398\u0395\u039c\u0391\u03a4\u039f\u03a3\n*\n*\/\n\nvar proSubjSelect = ($(\"#reasoning\").getValue().length > 0) ? 1: 0; \/\/\u03b1\u03bd \u03b5\u03c0\u03b9\u03c3\u03c4\u03c1\u03ad\u03c6\u03bf\u03c5\u03bc\u03b5 \u03c3\u03c4\u03b7\u03bd \u03c6\u03bf\u03c1\u03bc\u03b1 \u03ba\u03b1\u03b9 \u03c5\u03c0\u03ac\u03c1\u03c7\u03b5\u03b9 \u03c0\u03c1\u03bf\u03ba. \u0398\u03ad\u03bc\u03b1\nvar reasoningId =   ($(\"#reasoning\").getValue().length > 0 ) ? $(\"#reasoning\").getValue(): -1;\nvar reasoningText;\nvar reasoningDDChange = 0;\n\n$(\"#reasoningInsert\").setValue(0); \/\/\u03b1\u03c1\u03c7\u03b9\u03ba\u03ae \u03c4\u03b9\u03bc\u03ae\n\n$(\"#reasoning\").hide();\n$(\"#reasoningDropDown\").hide();\n\n$(\"#SUBJ\").dblclick(function(){\n\n\t$(\"#reasoning\").toggle();\n\t$(\"#reasoningDropDown\").toggle();\n  \n\treasoningId = $(\"#reasoning\").getValue();\n  \treasoningText = $(\"#reasoning\").getText();\n\n});\n\n$(\"#reasoning\").dblclick(function(){\n\n\t$(\"#reasoning\").hide();\n\t$(\"#reasoningDropDown\").hide();\n  \n\t \n});\n\n\n\n\n\n\/\/\n\/\/\n\n$(\"#reasoning\").focusout(function(){\n\n  if( $(\"li.pmdynaform-suggest-list-keyboard\").length == 0 ){\n    \n     if( $(\"#reasoning\").getValue().indexOf($(\"#reasoning\").getText()) === 0 ) {\n   \n     \tif($(\"#reasoning\").getValue().length == 0){\n    \t\tproSubjSelect = 0;\n       \t\treasoningId = -1;\n            $(\"#reasoningInsert\").setValue(0);\/\/ do nothing in php trigger\n     \t} \n       else if($(\"#reasoning\").getText()!= reasoningText){ \/\/ \u03b1\u03bb\u03bb\u03b1\u03b3\u03ae \u03bc\u03b5 \u03c0\u03bb\u03b7\u03ba\u03c4\u03c1\u03bf\u03bb\u03cc\u03b3\u03b7\u03c3\u03b7 \u03ba\u03b1\u03b9 \u03cc\u03c7\u03b9 \u03b1\u03c0\u03bf \u03c4\u03bf DropDown \n         if(checkInsOrUpd(\"reasoning\", reasoningId,\"#reasoningInsert\", \" \u03c4\u03bf \u03a0\u03a1\u039f\u0395\u03a0\u0399\u039b\u0395\u0393\u039c\u0395\u039d\u039f \u0398\u0395\u039c\u0391:\" )){\n            \t$(\"#SUBJ\").setValue($(\"#reasoning\").getText()); \n            \treasoningText =  $(\"#reasoning\").getText();\n           \t\tif($(\"#reasoningInsert\").getValue() == 1) reasoningId = -1; \/\/ no ID for insert\n          }\n       }\n       \n\n     }\n    \n\n  }\n  else {\n    \n\t$('ul.pmdynaform-suggest-list').on('mousedown', 'li', function(e) {\n       var liSelector =  $(\"li.list-group-item.pmdynaform-suggest-list-keyboard\").children().get(0) ;\n       reasoningId = $(liSelector).attr(\"data-value\");\n       reasoningText = $(liSelector).html();\n      \n       $(\"#SUBJ\").setValue(reasoningText);\n        \n       proSubjSelect = 1; \/\/ \u03ad\u03c7\u03b5\u03b9 \u03b5\u03c0\u03b9\u03bb\u03b5\u03b3\u03b5\u03af \u03ad\u03bd\u03b1 \u03b8\u03ad\u03bc\u03b1\n            \n\t});\n  } \n  \n});\n\n\n   \n\n\n\/\/\n\/\/\n \n$('#reasoningDropDown').setOnchange(function(e) {\n   \n   \n      \tvar resDDval, resDDte