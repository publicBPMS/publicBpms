n      contComm = $(\"#contComm\"),        \n      allFields = $( [] ).add( contPerson ).add( contAddress ).add( contPOBox ).add( contArea ).add( contZip ).add( contDep1 ).add( contDep2 ).add( contEmail ).add( contPhone ).add( contFax ).add( contDesc ).add( contComm ),\n      tips = $( \".validateTips\" ),\n\t  $calSelector ;\n   \n    function updateTips( t ) {\n      tips\n        .text( t )\n        .addClass( \"ui-state-highlight\" );\n      setTimeout(function() {\n        tips.removeClass( \"ui-state-highlight\", 1500 );\n      }, 500 );\n    }\n \n    function checkLength( o, n, min, max ) {\n      if ( o.val().length > max || o.val().length < min ) {\n        o.addClass( \"ui-state-error\" );\n        updateTips( \"\u03a4\u03bf \u03bc\u03ae\u03ba\u03bf\u03c2 \" + n + \" \u03c0\u03c1\u03ad\u03c0\u03b5\u03b9 \u03bd\u03b1 \u03b5\u03af\u03bd\u03b1\u03b9 \u03bc\u03b5\u03c4\u03b1\u03be\u03cd \" +\n          min + \" and \" + max + \".\" );\n        return false;\n      } else {\n        return true;\n      }\n    }\n \n    function checkRegexp( o, regexp, n ) {\n      if ( !( regexp.test( o.val() ) ) ) {\n        o.addClass( \"ui-state-error\" );\n        updateTips( n );\n        return false;\n      } else {\n        return true;\n      }\n    }\n \n    function addDetails() {\n      var valid = true;\n      allFields.removeClass( \"ui-state-error\" );\n \n      valid = valid && (contPerson.val().length >0) ? checkLength( contPerson, \"\u03c4\u03bf\u03c5 \u03bf\u03bd\u03cc\u03bc\u03b1\u03c4\u03bf\u03c2\", 6, 32 ): true;\n      valid = valid && (contEmail.val().length >0) ? checkLength( contEmail, \"email\", 6, 80 ): true;\n \n      valid = valid && (contEmail.val().length >0) ? checkRegexp( contEmail, emailRegex, \"\u03a3\u03c6\u03ac\u03bb\u03bc\u03b1 \u03c3\u03c4\u03bf e-mail\" ): true;\n     \n \n      if ( valid ) {\n        $calSelector.val( \"\u0395\u03c0\u03b1\u03c6\u03ae\\t:\\t\"+ contPerson.val()+\"\\n\"+\n        \t\t\"\u039f\u03b4\u03cc\u03c2\\t:\\t\"+