ckground-color\",\"#D8D8C0\");\n$(\"#gridBpmsCc\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridBpmsCc\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n\n\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelSigners\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelAssign\").css(\"background-color\",\"#D8D8C0\");\n\n\n\n\/\/\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) {\n    \t$(selector).show();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) {\n        $(\"#sxetikaButton\").show();\n        $(selector).hide();\n      }\n    }\n    else {\n    \t$(selector).hide();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) $(\"#sxetikaButton\").hide();\n    }\n  }\n}\n\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelAssign\");\npanelDisplay(\"panelDocFiles\");\npanelDisplay(\"panelSxtFiles\");\npanelDisplay(\"panelInitDocFiles\");\npanelDisplay(\"panelInitRcpts\");\n\/\/\n\/\/\n\n $(\"#bpmsUsers\").hide();\n $(\"#authorities\").hide();\n\n\/\/\n\/\/ remove links for sign, but it  remains in panelDocFiles variable\n$(\"a.signDoc\").remove();\n\/\/\n\n\/\/\n$(\"#sxetikaButton\").click(function(){$(\"#panelSxtFiles\").toggle();});\n\/\/\n\n\/\/\n$(\"#dialogContactDetailsForm\").hide();\n\n\/\/get form id\nvar formId = $(\"form\")[0].id;\nvar form = getFormById(formId);\n\n$(\"#reloadFormId\").setValue(formId);\n\n\/\/\n\n\/\/if($(\"#countRcpts\").getValue() == 1 ) $(\"#submit2\").hide();\n\n$(\"#gridAttFiles\").hide();\n$(\"#synimmenaButton\").click(function(){$(\"#gridAttFiles\").toggle();});\n\n\n\n\/* v.2.0 1\/8\/2016\n\n\/---------------------------\n\/ \u0394\u03b9\u03b1\u03c7\u03b5\u03af\u03c1\u03b9\u03c3\u03b7 \u03c4\u03bf\u03c5 \u03b1\u03c0\u03bf\u03c3\u03c4\u03bf\u03bb\u03ad\u03b1 - \u0395\u03bd\u03b7\u03bc\u03ad\u03c1\u03c9\u03c3\u03b7 \u03b5\u03c0\u03b9\u03c0\u03bb\u03ad\u03bf\u03bd \u03c3\u03c4\u03bf\u03b9\u03c7\u03b5\u03af\u03c9\u03bd ( AUTHORITY )\n\/ \u03c4\u03bf \u03c3\u03c5\u03b3\u03ba\u03b5\u03ba\u03c1\u03b9\u03bc\u03ad\u03bd\u03bf \u03bc\u03ad\u03c1\u03bf\u03c2 \u03c4\u03bf\u03c5 script \u03c0\u03b5\u03c1\u03b9\u03bf\u03c1\u03af\u03b6\u03b5\u03c4\u03b1\u03b9 \u03c3\u03b5 \u03bc\u03b9\u03b1 \u03ba\u03b1\u03b9 \u03bc\u03cc\u03bd\u03bf \u03b3\u03c1\u03b1\u03bc\u03bc\u03ae \n\/ \u03ba\u03b1\u03b9 \u03b5\u03c0\u03bf\u03bc\u03ad\u03bd\u03c9\u03c2 \u03b4\u03b9\u03b1\u03c6\u03bf\u03c1\u03bf\u03c0\u03bf\u03b9\u03b5\u03af\u03c4\u03b1\u03b9 \u03bb\u03af\u03b3\u03bf \u03b1\u03c0\u03bf \u03c4\u03b7\u03bd \u03b3\u03b5\u03bd\u03b9\u03ba\u03ae \u03c4\u03bf\u03c5 \u03bc\u03bf\u03c1\u03c6\u03ae \u03c0\u03bf\u03c5 \u03c3\u03c5\u03bd\u03b1\u03bd\u03c4\u03ac\u03c4\u03b1\u03b9 \u03c3\u03b5 \u03ac\u03bb\u03bb\u03b5\u03c2 \u03c6\u03bf\u03c1\u03bc\u03b5\u03c2 ( \u03c0\u03b1\u03c1\u03b1\u03bb\u03ae\u03c0\u03c4\u03b5\u03c2 )\n*\/\n\n\n var  selRcpts = $(\"#authorities\") ;\n\n\nvar aTrace  = [];\n\taTrace[1]  = {cNameId:-1,cDetails:'',insOrUpd:0};\n \n\n$(\"#bpmsUsersButton\").click(function(){\n  $(\"#bpmsUsers\").show();\n  $(\"#authorities\").hide();\n});\n\n$(\"#otherUsersButton\").click(function(){\n  \n  $(\"#bpmsUsers\").hide();\n  $(\"#authorities\").show();\n});\n\n\/\/\n\/\/ bpmsUsers\n\/\/ ON ADDROW\n$(\"#bpmsUsers\").onAddRow(function(aNewRow, oGrid, rowIndex) {\n  \n    $(\"#bpmsUsers\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n});\n\/\/  \n\n\n\n\/\/authority\n\/\/ ON ADDROW\n\n$(\"#authorities\").onAddRow(function(aNewRow, oGrid, rowIndex) {\n  \n  \n  \n  aTrace.push({cNameId:-1,cDetails:\"\",insOrUpd:0});\n \n  var scopeVal = $(\"#SCOPE\").getValue();\n  $(\"#authorities\").setValue(scopeVal, rowIndex,7); \/\/\u03c0\u03c1\u03bf\u03b5\u03c4\u03bf\u03b9\u03bc\u03b1\u03c3\u03af\u03b1 SQL  \u03b3\u03b9\u03b1 \u03c4\u03b7\u03bd \u03b5\u03c0\u03b9\u03bb\u03bf\u03b3\u03ae \u03c4\u03c9\u03bd authorities \n  \n setGridEvents(); \/\/ events \u03b1\u03c0\u03bf \u03c4\u03b7\u03bd \u03b1\u03c1\u03c7\u03ae\n  \n  for(var i=1;i< aTrace.length;i++){\n  \n  }\n  $(\"#authorities\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n});\n\n\/\/\n\/\/\n\n$(\"#authorities\").onDeleteRow(function(oGrid, aRow, rowIndex){\n\n  \taTrace.splice(rowIndex,1);\n\t\n \tsetGridEvents(); \/\/ events \u03b1\u03c0\u03bf \u03c4\u03b7\u03bd \u03b1\u03c1\u03c7\u03ae\n  \n}); \n\/\/\n\/\/\nfunction setGridEvents(){\n\t\n  var nGridRows = $(\"#authorities\").getNumberRows();\n  \n    \n  if(nGridRows !== 0){\n    \n    \t\/\/ \u03c0\u03ac\u03bd\u03c4\u03b1 \u03b8\u03b1 \u03c0\u03c1\u03ad\u03c0\u03b5\u03b9 \u03bd\u03b1 \u03b5\u03af\u03bd\u03b1\u03b9 \u03ba\u03c1\u03c5\u03c6\u03ac  \t \n         $(\"#authorities\").hideColumn(6); \n \t\t $(\"#authorities\").hideColumn(7); \n    \n    \t\/\/remove first events\n        for (var i = 1; i <= nGridRows; i++) {\n            var $cDetails = $(\"#form\\\\[authorities\\\\]\\\\[\"+i+\"\\\\]\\\\[cDetails\\\\]\");\n            var $cDetails_label = $(\"#form\\\\[authorities\\\\]\\\\[\"+i+\"\\\\]\\\\[cDetails_label\\\\]\");\n  \t\t\tvar $cNameId =  $(\"#form\\\\[authorities\\\\]\\\\[\"+i+\"\\\\]\\\\[cNameId\\\\]\");\n          \tvar $cNameId_label =  $(\"#form\\\\[authorities\\\\]\\\\[\"+i+\"\\\\]\\\\[cNameId_label\\\\]\");\n          \n    \t\t$cDetails.off('click dblclick focusout');\n            \n          \t$cNameId_label.off('click focusout change');\n            \n        }\n        \/\/add events\n        for (var i = 1; i <= nGridRows; i++) {\n          \n            var $cDetails = $(\"#form\\\\[authorities\\\\]\\\\[\"+i+\"\\\\]\\\\[cDetails\\\\]\");\n            var $cDetails_label = $(\"#form\\\\[authorities\\\\]\\\\[\"+i+\"\\\\]\\\\[cDetails_label\\\\]\");\n  \t\t\tvar $cNameId =  $(\"#form\\\\[authorities\\\\]\\\\[\"+i+\"\\\\]\\\\[cNameId\\\\]\");\n            var $cNameId_label =  $(\"#form\\\\[authorities\\\\]\\\\[\"+i+\"\\\\]\\\\[cNameId_label\\\\]\");\n          \n  \t\t\t$cDetails.click({ gridRow: i},function(event) {\n      \t\t\t\t\tvar strSel = \"#form\\\\[authorities\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[cDetails\\\\]\";\n              \t\t\tlargeDet(strSel);\n    \t\t});\n   \n  \t\t\t$cDetails.dblclick({ gridRow: i},function(event){\n       \t\t\t\t\tvar strSel = \"#form\\\\[authorities\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[cDetails\\\\]\";\n                        $(\"#gridContSelector\").setValue(strSel); \/\/\u03c7\u03c1\u03b5\u03b9\u03ac\u03b6\u03b5\u03c4\u03b1\u03b9 \u03ad\u03bd\u03b1 hiden field\n       \t\t\t\t\t$( \"#dialog-form\" ).dialog(\"open\");\n      \t\t\t\t\t\n            });\n  \n  \t\t\t$cDetails.focusout({ gridRow: i},function(event) {\n              \t\t\tviewGridFields();\n      \t\t\t\t\tcDetailsMngmnt(\"authorities\",event.data.gridRow);\n              \t\t\t\n    \t\t});\n\n            $cDetails.change({ gridRow: i},function(event) {\n              \t\t\t\n              \t\t\tif($(\"#authorities\").getValue(event.data.gridRow,6) != 1){\n      \t\t\t\t\t\t$(\"#authorities\").setValue(2,event.data.gridRow,6); \/\/update\n             \t\t\t }\n              \t\t\t\n    \t\t});\n  \n  \t\t\t$cNameId_label.change({ gridRow: i},function(event) {\n              \t\t\n      \t\t\t\t\tauthorityMngmnt(\"authorities\",event.data.gridRow);\n              \t\t\t\n    \t\t});\n          \n            $cNameId_label.focusout({ gridRow: i},function(event) {\n              \t\t\t\n              \t\t\tviewGridFields();\n      \t\t\t\t\t\n    \t\t});\n          \n            $cNameId_label.click({ gridRow: i},function(event) {\n            \n     \t\t\tif( $(\"#authorities\").getValue(event.data.gridRow,1).indexOf($cNameId_label.val()) == -1 ) {\n     \t\t\t\taTrace[event.data.gridRow][\"cNameId\"]  =  $(\"#authorities\").getValue(event.data.gridRow,1);\n       \t\t\t\taTrace[event.data.gridRow][\"cDetails\"]  =  $(\"#authorities\").getValue(event.data.gridRow,2);\n       \t\t\t\t$(\"#authorities\").setValue(0,event.data.gridRow,6); \/\/no ins or upd\n       \t\t\t\n            \t}\n       \n      \n            \tlargeName();\n          \t});  \n          \n        }\n    \n    }\n\n}\n\n\n\nfunction largeName(){\n  var cRows = $(\"#authorities\").getNumberRows();\n  \n  $(\"#authorities\").hideColumn(3); \n  $(\"#authorities\").hideColumn(4); \n  $(\"#authorities\").hideColumn(5); \n  for (var i = 1; i <= cRows; i++) {\n     $(\"div#\\\\[authorities\\\\]\\\\[\" + i + \"\\\\]\\\\[cNameId\\\\]\").parent().parent().css(\"width\", \"50%\");\n     $(\"div#\\\\[authorities\\\\]\\\\[\" + i + \"\\\\]\\\\[cDetails\\\\]\").parent().parent().css(\"width\", \"30%\");\n     $(\"#form\\\\[authorities\\\\]\\\\[\" + i+ \"\\\\]\\\\[cDetails\\\\]\").attr(\"rows\", \"1\");\n  }\n}\n\n\nfunction largeDet(rSel){\n  \n  var cRows = $(\"#authorities\").getNumberRows();\n  $(\"#authorities\").hideColumn(3);\n  $(\"#authorities\").hideColumn(4); \n  $(\"#authorities\").hideColumn(5);\n  $(rSel).attr(\"rows\", \"14\");\n  for (var i = 1; i <= cRows; i++) {\n    $(\"div#\\\\[authorities\\\\]\\\\[\" + i + \"\\\\]\\\\[cNameId\\\\]\").parent().parent().css(\"width\", \"30%\");\n   $(\"div#\\\\[authorities\\\\]\\\\[\" + i + \"\\\\]\\\\[cDetails\\\\]\").parent().parent().css(\"width\", \"50%\");\n  }\n  \n}\n\n\n\/\/$(\"#authorities\").focusout(function(){\nfunction viewGridFields() {\n   var cRows = $(\"#authorities\").getNumberRows();\n\t$(\"#authorities\").showColumn(3);\n    $(\"#authorities\").showColumn(4);\n     $(\"#authorities\").showColumn(5);\n   for (var i = 1; i <= cRows; i++) {\n     $(\"div#\\\\[authorities\\\\]\\\\[\" + i + \"\\\\]\\\\[cNameId\\\\]\").parent().parent().css(\"width\", \"30%\");\n     $(\"div#\\\\[authorities\\\\]\\\\[\" + i + \"\\\\]\\\\[cDetails\\\\]\").parent().parent().css(\"width\", \"30%\");\n     $(\"#form\\\\[authorities\\\\]\\\\[\" + i+ \"\\\\]\\\\[cDetails\\\\]\").attr(\"rows\", \"1