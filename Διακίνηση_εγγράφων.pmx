        <dyn_content><![CDATA[{"name":"\u0394\u03b9\u03b5\u03ba\u03c0\u03b5\u03c1\u03b1\u03af\u03c9\u03c3\u03b7 \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03c9\u03bd 2.0","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"55731201157a80d9c9136b9013151541","name":"\u0394\u03b9\u03b5\u03ba\u03c0\u03b5\u03c1\u03b1\u03af\u03c9\u03c3\u03b7 \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03c9\u03bd 2.0","description":"","mode":"edit","script":{"type":"js","code":"$(\"form\").css(\"background-color\", \"#DEE0EC\");\n\n$(\"#form\\\\[submit1\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\n$(\"#title1\").css(\"color\",\"#ffffff\");\n$(\"#title1\").css(\"background-color\",\"#428bca\");\n\n\n$(\"#panelSxtFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n\n$(\"#gridBpmsRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridBpmsRcpts-body\").children().children().css(\"background-color\",\"#D8D8C0\");\n$(\"#gridOtherRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridOtherRcpts-body\").children().children().css(\"background-color\",\"#D8D8C0\");\n\n$(\"#gridBpmsRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridBpmsRcpts\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridOtherRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridOtherRcpts\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n\n\/\/\n\/\/\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) \n    \t$(selector).show();\n  \telse\n    \t$(selector).hide();\n  }\n}\n\npanelDisplay(\"panelRcpts\");\n\npanelDisplay(\"panelDocFiles\");\n\n\n\/\/\n\/\/\n\n $(\"#gridBpmsRcpts\").hide();\n $(\"#gridOtherRcpts\").hide();\n\n\n\n$(\"#bpmsUsers\").click(function(){\n  $(\"#gridBpmsRcpts\").show();\n  $(\"#gridOtherRcpts\").hide();\n});\n\n$(\"#otherUsers\").click(function(){\n  \n  $(\"#gridBpmsRcpts\").hide();\n  $(\"#gridOtherRcpts\").show();\n});\n\n\n\/\/\n\/* v.2.0 1\/8\/2016\n\/\n\/\u0394\u03b9\u03b1\u03c7\u03b5\u03af\u03c1\u03b9\u03c3\u03b7 \u03c4\u03bf\u03c5 \u03b1\u03c0\u03bf\u03c3\u03c4\u03bf\u03bb\u03ad\u03b1 - \u0395\u03bd\u03b7\u03bc\u03ad\u03c1\u03c9\u03c3\u03b7 \u03b5\u03c0\u03b9\u03c0\u03bb\u03ad\u03bf\u03bd \u03c3\u03c4\u03bf\u03b9\u03c7\u03b5\u03af\u03c9\u03bd ( CONTACT )\n\/ OTHER GRID setup\n\/\n*\/\nfunction setupRows(){\n\tvar cRows = $(\"#gridOtherRcpts\").getNumberRows();\n\tvar sVal = \"\";\n\n  for (var i = 1; i <= cRows; i++) {\n    var $cDetails = $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+i+\"\\\\]\\\\[cDetails\\\\]\");\n  \t$(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+i+\"\\\\]\\\\[cNameId_label\\\\]\").click(largeName);\n\/\/\n  \tsVal = $(\"#gridOtherRcpts\").getValue(i, 2);\n  \tsVal = sVal.replace(\/\\\\t\/g, \"\\t\");\n  \tsVal = sVal.replace(\/\\\\n\/g, \"\\n\");\n    \n  \t$(\"#gridOtherRcpts\").setValue(sVal, i,2);\n    \n  \t$cDetails.click({ gridRow: i}, function(event) {\n      \t\t\t\t\tvar strSel = \"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[cDetails\\\\]\";\n      \t\t\t\t\tlargeDet(strSel);\n    \t\t\t\t});\n    $cDetails.dblclick({ gridRow: i}, function(event){\n       \t\t\t\t\tvar strSel = \"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[cDetails\\\\]\";\n                        $(\"#gridContSelector\").setValue(strSel); \/\/\u03c7\u03c1\u03b5\u03b9\u03ac\u03b6\u03b5\u03c4\u03b1\u03b9 \u03ad\u03bd\u03b1 hiden field\n       \t\t\t\t\t$( \"#dialog-form\" ).dialog(\"open\");\n      \t\t\t\t\t\n            \t\t\t});\n  }\n} \/\/ setupRows\n \n$(\"form\").setOnchange(function(){\n  debugger;\n  setupRows;\n  \n});\nsetupRows(); \/\/on load form\n\n\n\/\/ ON ADDROW\n$(\"#gridOtherRcpts\").onAddRow(function(aNewRow, oGrid, rowIndex) {\n  \n   $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+ rowIndex +\"\\\\]\\\\[cNameId\\\\]\").click(largeName);  \n\/\/\n  var $cDetails = $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+rowIndex+\"\\\\]\\\\[cDetails\\\\]\");\n\n  sVal = aNewRow[1].getValue(); \n  sVal = sVal.replace(\/\\\\t\/g, \"\\t\");\n  sVal = sVal.replace(\/\\\\n\/g, \"\\n\");\n  aNewRow[1].setValue(sVal);\n  \n  setupRows();\n  $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+ rowIndex +\"\\\\]\\\\[cDetails\\\\]\").click({ gridRow: rowIndex}, function(event) {\n      \t\t\t\t\tvar strSel = \"#form\\\\[gridOtherRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[cDetails\\\\]\";\n      \t\t\t\t\tlargeDet(strSel);\n    \t\t\t\t});\n  \n \n});\n\nfunction largeName(){\n  var cRows = $(\"#gridOtherRcpts\").getNumberRows();\n \n  $(\"#gridOtherRcpts\").hideColumn(3); \n  $(\"#gridOtherRcpts\").hideColumn(4); \n  $(\"#gridOtherRcpts\").hideColumn(5); \n  for (var i = 1; i <= cRows; i++) {\n   $(\"div#\\\\[gridOtherRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[cNameId\\\\]\").parent().parent().css(\"width\", \"50%\");\n  }\n}\n\n\nfunction largeDet(rSel){\n  var cRows = $(\"#gridOtherRcpts\").getNumberRows();\n  $(\"#gridOtherRcpts\").hideColumn(3);\n  $(\"#gridOtherRcpts\").hideColumn(4); \n  $(\"#gridOtherRcpts\").hideColumn(5);\n  $(rSel).attr(\"rows\", \"11\");\n  for (var i = 1; i <= cRows; i++) {\n   $(\"div#\\\\[gridOtherRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[cDetails\\\\]\").parent().parent().css(\"width\", \"50%\");\n  }\n  \n}\n\n\n$(\"#gridOtherRcpts\").focusout(function(){\n   var cRows = $(\"#gridOtherRcpts\").getNumberRows();\n\t$(\"#gridOtherRcpts\").showColumn(3);\n    $(\"#gridOtherRcpts\").showColumn(4);\n     $(\"#gridOtherRcpts\").showColumn(5);\n   for (var i = 1; i <= cRows; i++) {\n     $(\"div#\\\\[gridOtherRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[cNameId\\\\]\").parent().parent().css(\"width\", \"30%\");\n     $(\"div#\\\\[gridOtherRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[cDetails\\\\]\").parent().parent().css(\"width\", \"25%\");\n     $(\"#form\\\\[gridOtherRcpts\\\\]\\\\[\" +  i+ \"\\\\]\\\\[cDetails\\\\]\").attr(\"rows\", \"1\");\n   }\n                      \n  });\n\n\n\/* v.2.0 1\/9\/2016\n\/\n\/\u0394\u03b9\u03b1\u03c7\u03b5\u03af\u03c1\u03b9\u03c3\u03b7 \u03b5\u03c3\u03c9\u03c4\u03b5\u03c1\u03b9\u03ba\u03ce\u03bd \u03c0\u03b1\u03c1\u03b1\u03bb\u03b7\u03c0\u03c4\u03ce\u03bd. \u0394\u03b9\u03b1\u03c6\u03bf\u03c1\u03b5\u03c4\u03b9\u03ba\u03ae \u03b4\u03b9\u03b1\u03c7\u03b5\u03af\u03c1\u03b9\u03c3\u03b7 BPMS \u03ba\u03b1\u03b9 \u03b5\u03c3\u03c9\u03c4\u03b5\u03c1\u03b9\u03ba\u03ce\u03bd \u03c7\u03c1\u03b7\u03c3\u03c4\u03ce\u03bd \u03c0\u03bf\u03c5 \u03b4\u03b5\u03bd \u03c5\u03c0\u03bf\u03c3\u03c4\u03b9\u03c1\u03af\u03b6\u03bf\u03bd\u03c4\u03b1\u03b9 \u03b1\u03c0\u03bf \u03c4\u03bf BPMS\n\/ BPMS  setup\n\/\n*\/\nfunction setupBpmsRows(){\n\tvar cRows = $(\"#gridBpmsRcpts\").getNumberRows();\n\tvar sVal = \"\";\n\n  for (var i = 1; i <= cRows; i++) {\n    var $iDetails = $(\"#form\\\\[gridBpmsRcpts\\\\]\\\\[\"+i+\"\\\\]\\\\[iDetails\\\\]\");\n    $(\"#gridBpmsRcpts\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n  \t\n\tif($(\"#gridBpmsRcpts\").getValue(i, 6)) {\n\t\t$(\"div#\\\\[gridBpmsRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[iCheckEmail\\\\]\").parent().parent().css(\"width\", \"0\");\n\t\t$(\"div#\\\\[gridBpmsRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[iSendHow\\\\]\").parent().parent().css(\"width\", \"0\");\n\t} else {\n\t\t$(\"div#\\\\[gridBpmsRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[iCheckEmail\\\\]\").parent().parent().css(\"width\", \"10%\");\n\t\t$(\"div#\\\\[gridBpmsRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[iSendHow\\\\]\").parent().parent().css(\"width\", \"10%\");\n\t}\n\/\/\n  \tsVal = $(\"#gridBpmsRcpts\").getValue(i, 2);\n  \tsVal = sVal.replace(\/\\\\t\/g, \"\\t\");\n  \tsVal = sVal.replace(\/\\\\n\/g, \"\\n\");\n  \t$(\"#gridBpmsRcpts\").setValue(sVal, i,2);\n    \n  \t$iDetails.click({ gridRow: i}, function(event) {\n      \t\t\t\t\tvar strSel = \"#form\\\\[gridBpmsRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[iDetails\\\\]\";\n      \t\t\t\t\tlargeiDet(strSel);\n    \t\t\t\t});\n   \/\/ $iDetails.dblclick({ gridRow: i}, function(event){\n   \/\/    \t\t\t\t\tvar strSel = \"#form\\\\[gridBpmsRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[iDetails\\\\]\";\n   \/\/                     $(\"#gridContSelector\").setValue(strSel); \/\/\u03c7\u03c1\u03b5\u03b9\u03ac\u03b6\u03b5\u03c4\u03b1\u03b9 \u03ad\u03bd\u03b1 hiden field\n   \/\/    \t\t\t\t\t$( \"#dialog-form\" ).dialog(\"open\");\n  \/\/    \t\t\t\t\t\n   \/\/         \t\t\t});\n  }\n} \/\/ setupBpmsRows\n \n$(\"form\").setOnchange(setupBpmsRows);\nsetupBpmsRows(); \/\/on load form\n\n\n\/\/ ON ADDROW\n$(\"#gridBpmsRcpts\").onAddRow(function(aNewRow, oGrid, rowIndex) {\n  \n \/\/\n  var $iDetails = $(\"#form\\\\[gridBpmsRcpts\\\\]\\\\[\"+rowIndex+\"\\\\]\\\\[iDetails\\\\]\");\n  setupBpmsRows();\n  var sVal = aNewRow[1].getValue(); \n  sVal = sVal.replace(\/\\\\t\/g, \"\\t\");\n  sVal = sVal.replace(\/\\\\n\/g, \"\\n\");\n  aNewRow[1].setValue(sVal);\n  $(\"#form\\\\[gridBpmsRcpts\\\\]\\\\[\"+ rowIndex +\"\\\\]\\\\[iDetails\\\\]\").click({ gridRow: rowIndex}, function(event) {\n      \t\t\t\t\tvar strSel = \"#form\\\\[gridBpmsRcpts\\\\]\\\\[\"+event.data.gridRow+\"\\\\]\\\\[iDetails\\\\]\";\n      \t\t\t\t\tlargeiDet(strSel);\n    \t\t\t\t});\n\n});\n\n\nfunction largeiDet(rSel){\n  var cRows = $(\"#gridBpmsRcpts\").getNumberRows();\n  $(\"#gridBpmsRcpts\").hideColumn(3);\n  $(\"#gridBpmsRcpts\").hideColumn(4); \n  $(\"#gridBpmsRcpts\").hideColumn(5);\n  $(rSel).attr(\"rows\", \"8\");\n  for (var i = 1; i <= cRows; i++) {\n   $(\"div#\\\\[gridBpmsRcpts\\\\]\\\\[\" + i + \"\\\\]\\\\[iDetails\\\\]\").parent().parent().css(\"width\", \"50%\");\n  }\n  \n}\n\n\n$(\"#gridBpmsRcpts\").focusout(function(){\n   var cRows = $(\"#gridBpmsRcpts\").getNumberRows();\n\t$(\"#gridBpmsRcpts\").showColumn(3);\n    $(\"#gridBpmsRcpts\").showColumn(4);\n    