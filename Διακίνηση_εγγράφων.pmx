$reslt = executeQuery($query);


if(is_array($reslt) and count($reslt) >0 ){
	
	
	if( (empty(@=gridListFiles[1]) or empty(@=gridListFiles[1]['fUid'])) and count(@=gridListFiles) == 1 and ( !is_array(@=gridOrigListFiles) or count(@=gridOrigListFiles)  <= 1)){ 
		
		@=gridOrigListFiles = array('fakeItem'); // για να παρακολουθώ τις διαγραφές
		 @=gridListFiles = array();
	}
	 

	
	foreach ($reslt as $key => $value) {
		

		
		if( array_search($value['APP_DOC_UID'], @=gridOrigListFiles) === false ){ 
			// add this file to listFiles
	
				@=gridListFiles[$key]['fUid'] = $value['APP_DOC_UID'];
				@=gridListFiles[$key]['fName'] = $value['APP_DOC_FILENAME'];
				@=gridListFiles[$key]['fCat'] = 0 ;// άγνωστο 
				@=gridListFiles[$key]['isMain'] = 0;
				
				@=gridOrigListFiles[$key] = $value['APP_DOC_UID'];
				
			    $nFormRefreash = true;
			
		}
		else if(array_search($value['APP_DOC_UID'], array_column(@=gridListFiles, 'fUid' )) === false ){ 
			// not in ListFiles.Delete this file from db
					$oAppDoc->remove($value['APP_DOC_UID'], $value['DOC_VERSION']);
				
					$loIdx = array_search($value['APP_DOC_UID'],@=gridOrigListFiles);
					unset(@=gridOrigListFiles[$loIdx]);
			
				}
				else	{ //update db from ListFiles
					
					$gIdx = array_search($value['APP_DOC_UID'], array_column( @=gridListFiles , 'fUid' )) ; // attension to array indexes
					
					$field = (@=gridListFiles[$gIdx+1]['isMain']) ? 'doc:' : 'att:' ;
					$field .= @=gridListFiles[$gIdx+1]['fCat_label'];
					
