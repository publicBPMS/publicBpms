        <dyn_content><![CDATA[{"name":"\u0394\u03b9\u03b1\u03b2\u03b9\u03b2\u03b1\u03c3\u03c4\u03b9\u03ba\u03cc 2.0","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"15542602557b3676d96d3f2054670082","name":"\u0394\u03b9\u03b1\u03b2\u03b9\u03b2\u03b1\u03c3\u03c4\u03b9\u03ba\u03cc 2.0","description":"","mode":"edit","script":{"type":"js","code":"$(\"form\").css(\"background-color\", \"#DEE0EC\");\n\n\n$(\".form-control\").css(\"font-size\", \"0.9em\");\n$(\".textlabel\").css(\"font-size\", \"0.88em\");\n$(\".title-column\").css(\"font-size\", \"0.88em\");\n$(\".pmdynaform-grid-title\").css(\"font-size\", \"0.9em\");\n\n\n$(\"#form\\\\[toSignSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\/\/$(\"#form\\\\[notToSignSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\/\/$(\"#form\\\\[reLoadFormSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\n$(\"#title1\").css(\"color\",\"#ffffff\");\n$(\"#title1\").css(\"background-color\",\"#428bca\");\n\n\n$(\"#panelDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n\n\n\n\n\/\/ panels\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) \n    \t$(selector).show();\n  \telse\n    \t$(selector).hide();\n  }\n}\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelDocFiles\");\n\n\/\/\n\n \n\n$(\".uploadNewVersion\").click(function(){\n  \t$(\"#dynaformPos\").setValue(3); \/\/ \u03b3\u03b9\u03b1 \u03c5\u03c0\u03bf\u03bb\u03bf\u03b3\u03b9\u03c3\u03bc\u03cc \u03c4\u03b7\u03c2 \u03c6\u03cc\u03c1\u03bc\u03b1\u03c2 \u03c3\u03c3\u03c4\u03bf \u03b2\u03ae\u03bc\u03b1 2 \u03c4\u03bf\u03c5 task\n    $(\"#uploadInputDocument\").setValue(1); \/\/allow to run inputdocument form\n\tselForm.saveForm();\n\n});\n\n\/\/-------------\n\n$(\".viewLCDocument\").remove();\n\/\/$(\".editDocument\").remove();\n$(\".signDocument\").remove();\n$(\".validDocument\").remove();\n\n\/\/-------------\n\n\n\n$(\"#diavivastikoText\").hide(); \/\/ keep hide until you change next code\n\n\/\/\n\/* reload form\n$(\"#reLoadFormSubmit\").hide();\n$(\"#notToSignSubmit\").hide();\n\n$(\"#diavivastikoText\").setOnchange(function(){\n  $(\"#reLoadFormSubmit\").show();\n  $(\"#toSignSubmit\").hide();\n  $(\"#notToSignSubmit\").hide();\n});\n$(\"#SUBJ\").setOnchange(function(){\n  $(\"#reLoadFormSubmit\").show();\n  $(\"#toSignSubmit\").hide();\n  $(\"#notToSignSubmit\").hide();\n});\n\n\n\n$(\"#reLoadFormSubmit\").click(function(){\n  $(\"#renderForm\").setValue(1);\n  $(\"form\").first().saveForm();\n});\n\nif($(\"#notToSign\").getValue() == 1) {\n  $(\"#reLoadFormSubmit\").hide();\n  $(\"#toSignSubmit\").hide();\n  $(\"#notToSignSubmit\").show();\n};\n*\/\n\n\/\/\n\/\/dialog\n$(\"#dialogPanel\").hide();\nvar url = \"\/lib\/jquery-ui-1.11.4\/jquery-ui.min.js\";\nvar fileref=document.createElement('script');\nfileref.setAttribute(\"type\",\"text\/javascript\");\nfileref.setAttribute(\"src\", url);\n$(\"head\")[0].appendChild(fileref);\n\/\/\nvar url = \"\/lib\/jquery-ui-themes-1.11.4\/start\/jquery-ui.min.css\";\nvar fileref=document.createElement(\"link\");\nfileref.setAttribute(\"rel\", \"stylesheet\");\nfileref.setAttribute(\"type\", \"text\/css\");\nfileref.setAttribute(\"href\", url);\n$(\"head\")[0].appendChild(fileref);\n\n\/\/hide the panel containing the \"newClientDialog\" DIV:\n$(\"#dialogPanel\").hide();\n\n\/\/Create the dialog with \"Yes\" and \"No\" buttons:\nfunction showConfirmDlg() {\n    var dlgContents = {\n        modal:     true,\n        resizable: false,\n        buttons: {\n            \"NAI\": function() {\n                $(this).dialog(\"close\");\n                callback(true);\n            },\n            \"OXI\": function() {\n                $(this).dialog(\"close\");\n                callback(false);\n            }\n        }\n    }\n    $(\"#newClientDialog\").dialog(dlgContents);\n}\n\n\/\/show dialog when the user clicks on the \"addClient\" button:\n\n$(\"#createDiav\").click(function(){\n  \n\/\/    showConfirmDlg();\n \n});\n  \n\/\/function to add new client to the grid if the user clicked on \"Yes\"\nfunction callback(answer) {\n    if (answer == true) {\n        $(\"#submit1\").setText(\"\u03a5\u03c0\u03bf\u03b3\u03c1\u03b1\u03c6\u03ae \u0394\u03b9\u03b1\u03b2\u03b9\u03b2\u03b1\u03c3\u03c4\u03b9\u03ba\u03bf\u03cd\");\n        $(\"form\").submitForm();\n    \t}\n  else {\n  }\n}"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title1","label":"\u0394\u03b9\u03b1\u03b2\u03b9\u03b2\u03b1\u03c3\u03c4\u03b9\u03ba\u03cc \u0395\u03b3\u03b3\u03c1\u03ac\u03c6\u03c9\u03bd","colSpan":12}],[{"type":"text","variable":"EDITOR_NAME","var_uid":"6625671385841b85f0f33a6075583672","dataType":"string","protectedValue":false,"id":"EDITOR_NAME","name":"EDITOR_NAME","label":"\u03a3\u03c5\u03bd\u03c4\u03ac\u03ba\u03c4\u03b7\u03c2","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"disabled","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"EDITOR_NAME","colSpan":6},{"type":"datetime","variable":"SEND_DATE","var_uid":"837374992576d8b9190f5f8010576680","dataType":"datetime","protectedValue":false,"id":"SEND_DATE","name":"SEND_DATE","label":"\u0397\u03bc. \u0394\u03b9\u03b1\u03b2\u03af\u03b2\u03b1\u03c3\u03b7\u03c2","placeholder":"","hint":"","required":true,"mode":"disabled","format":"DD-MM-YYYY","dayViewHeaderFormat":"MMMM YYYY","extraFormats":false,"stepping":1,"minDate":"","maxDate":"","useCurrent":"true","collapse":true,"locale":"","defaultDate":"","disabledDates":false,"enabledDates":false,"icons":{"time":"glyphicon glyphicon-time","date":"glyphicon