        <dyn_content><![CDATA[{"name":"\u03a0\u03c1\u03c9\u03c4. \u0391\u03bd\u03ac\u03c1\u03c4\u03b7\u03c3\u03b7 \u03b5\u03b9\u03c3\u03b5\u03c1\u03c7\u03bf\u03bc\u03ad\u03bd\u03bf\u03c5 v.2.3","description":"3\/2\/2017","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"87670898458943f8b869352022319551","name":"\u03a0\u03c1\u03c9\u03c4. \u0391\u03bd\u03ac\u03c1\u03c4\u03b7\u03c3\u03b7 \u03b5\u03b9\u03c3\u03b5\u03c1\u03c7\u03bf\u03bc\u03ad\u03bd\u03bf\u03c5 v.2.3","description":"3\/2\/2017","mode":"edit","script":{"type":"js","code":"$(\"form\").css(\"background-color\", \"#DEE0EC\");\n$(\"#panelScan\").css(\"background-color\",\"#DEE0EC\");\n\n\n$(\"#reasoning\").css(\"background-color\",\"#F8F00A\");\n$(\"#reasoningDropDown\").css(\"background-color\",\"#F8F00A\");\n\n$(\"#form\\\\[rcptsSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[protSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[specXaraktSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\n$(\"#title1\").css(\"color\",\"#ffffff\");\n$(\"#title1\").css(\"background-color\",\"#428bca\");\ndocument.getElementById('title1').style.borderRadius= \"8px\";\n\ndocument.getElementById(\"authority\").style.padding = \"5px 10px 20px 10px\";\ndocument.getElementById('authority').style.borderRadius= \"10px\";\ndocument.getElementById(\"gridDocFiles\").style.padding = \"5px 10px 20px 10px\";\ndocument.getElementById('gridDocFiles').style.borderRadius= \"10px\";\ndocument.getElementById(\"gridAttFiles\").style.padding = \"5px 10px 20px 10px\";\ndocument.getElementById('gridAttFiles').style.borderRadius= \"10px\";\n\n\n$(\"#form\\\\[rcptsSubmit\\\\]\").css(\"text-align\",\"center\");\n$(\"#form\\\\[protSubmit\\\\]\").css(\"text-align\",\"center\");\n\n\n\n$(\"#gridDocFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#gridDocFiles-body\").children().css(\"background-color\",\"#CCD8CC\");\n$(\"#gridAttFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#gridAttFiles-body\").children().css(\"background-color\",\"#CCD8CC\");\n$(\"#panelSxtFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n\n$(\"#authority\").css(\"background-color\",\"#D8D8C0\");\n$(\"#authority-body\").children().css(\"background-color\",\"#D8D8C0\");\n$(\"#bpmsUser\").css(\"background-color\",\"#D8D8C0\");\n$(\"#bpmsUser-body\").children().css(\"background-color\",\"#D8D8C0\");\n$(\"#gridBpmsRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridBpmsRcpts-body\").children().css(\"background-color\",\"#D8D8C0\");\n$(\"#gridOtherRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridOtherRcpts-body\").children().css(\"background-color\",\"#D8D8C0\");\n\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\".pmdynaform-control-checkbox-list\").css(\"background-color\",\"#D8D8C0\")\n\/\/\n\/\/\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) {\n    \t$(selector).show();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) {\n        $(\"#sxetikaButton\").show();\n        $(selector).hide();\n      }\n    }\n    else {\n    \t$(selector).hide();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) $(\"#sxetikaButton\").hide();\n    }\n  }\n}\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelDocFiles\");\npanelDisplay(\"panelSxtFiles\");\npanelDisplay(\"panelScan\");\n\n\/\/\n$(\"#dialogContactDetailsForm\").hide();\n\n$(\"#SCOPE\").hide();\n$(\"#gridDocFiles\").show();\n\n\/\/get form id\nvar formId = $(\"form\")[0].id;\nvar form = getFormById(formId);\n\n$(\"#reloadFormId\").setValue(formId);\n\n\/\/\n\n\/\/if($(\"#countRcpts\").getValue() == 1 ) $(\"#submit2\").hide();\n\n$(\"#gridAttFiles\").hide();\n$(\"#synimmenaButton\").click(function(){$(\"#gridAttFiles\").toggle();});\n\n\/\/\n$(\"#sxetikaButton\").click(function(){$(\"#panelSxtFiles\").toggle();});\n\/\/\n\n$(\"#chgrpBpmsTo\").show();\n$(\"#chgrpBpmsCc\").show();\n\n\/\/\n\n$(\"#rcptsSubmit\").click(function(){$(\"#epilogiProtSupporter\").setValue(1);}); \/\/1 \u03a7\u03b1\u03c1\u03b1\u03ba\u03c4\u03b7\u03c1\u03b9\u03c3\u03bc\u03cc\u03c2\n\n$(\"#protSubmit\").click(function(){$(\"#epilogiProtSupporter\").setValue(2);});  \/\/2 \u03a0\u03c1\u03c9\u03c4\u03bf\u03ba\u03cc\u03bb\u03bb\u03b7\u03c3\u03b7\n\n\n\n$(\"#chgrpBpmsTo\").setOnchange(function(){chgrpsChanged(1)}); \/\/ TO option\n$(\"#chgrpBpmsCc\").setOnchange(function(){chgrpsChanged(2)}); \/\/CC option\n\nfunction chgrpsChanged(s) {\n  if(s ==1 ) {\n    $(\"#chgrpBpmsTo\").getValue().forEach(function(i, item){$(\"#form\\\\[chgrpBpmsCc\\\\]\\\\[\"+i+\"\\\\]\").removeAttr('checked')});\n\n  }\n  else {\n    $(\"#chgrpBpmsCc\").getValue().forEach(function(i, item){$(\"#form\\\\[chgrpBpmsTo\\\\]\\\\[\"+i+\"\\\\]\").removeAttr('checked')});\n  }\n  \n  sCount = $(\"#chgrpBpmsTo\").getValue().length + $(\"#chgrpBpmsCc\").getValue().length;\n  $(\"#countRRcpts\").setValue(sCount);\n}\n\n\/\/ \u03ba\u03b1\u03b9 \u03b3\u03b9\u03b1 \u03bf\u03c4\u03b1\u03bd \u03b5\u03c0\u03b9\u03c3\u03c4\u03c1\u03ad\u03c6\u03bf\u03c5\u03bc\u03b5 \u03c3\u03c4\u03b7\u03bd \u03c6\u03bf\u03c1\u03bc\u03b1\nif($(\"#countRcpts\").getValue() == 1 ) { \/\/ \u03bc\u03cc\u03bd\u03bf \u03bc\u03b9\u03b1 \u03b4\u03b9\u03b5\u03cd\u03b8\u03c5\u03bd\u03c3\u03b7 \u03b5\u03be\u03c5\u03c0\u03b7\u03c1\u03b5\u03c4\u03b5\u03af\u03c4\u03b1\u03b9 \n  \n  if(!$(\"#docHasReg\").getValue() ){\n    $(\"#chgrpBpmsTo\").setValue([$(\"#uniqueRcpt\").getValue()]);\n\t$(\"#rcptsSubmit\").hide();\n    $(\"#epilogiProtSupporter\").setValue(2);\n  }\n  else {\n    $(\"#rcptsSubmit\").hide();\n    $(\"#protSubmit\").setValue(\"\u03a7\u03c1\u03ad\u03c9\u03c3\u03b7 \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03c9\u03bd\");\n    $(\"input.pmdynaform-control-checkgroup\").attr(\"disabled\", true);\n     $(\"#form\\\\[protSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n  }\n \n}\nelse { \/\/ \u03b5\u03c0\u03cc\u03bc\u03b5\u03bd\u03bf \u03c4\u03b1\u03c3\u03ba\n  \n  if($(\"#docHasReg\").getValue() == 1 ){\n    $(\"#rcptsSubmit\").hide();\n    \n    $(\"#protSubmit\").setValue(\"\u03a7\u03c1\u03ad\u03c9\u03c3\u03b7 \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03c9\u03bd\");\n    $(\"input.pmdynaform-control-checkgroup\").attr(\"disabled\", true);\n     $(\"#form\\\\[protSubmit\\\\]\").css(\"background-color\",\"#DAF7A6\");\n  \n  }\n}\n\n\n\n\/\/\n\/\/ \nif($(\"#hideUploadDocFile\").getValue() == 1 ) $(\"#gridDocFiles\").hide();\n\n\/*\n*\n* \u0394\u0399\u0391\u03a7\u0395\u0399\u03a1\u0399\u03a3\u0397 \u0398\u0395\u039c\u0391\u03a4\u039f\u03a3\n*\n*\/\n\nvar proSubjSelect = ($(\"#reasoning\").getValue().length > 0) ? 1: 0; \/\/\u03b1\u03bd \u03b5\u03c0\u03b9\u03c3\u03c4\u03c1\u03ad\u03c6\u03bf\u03c5\u03bc\u03b5 \u03c3\u03c4\u03b7\u03bd \u03c6\u03bf\u03c1\u03bc\u03b1 \u03ba\u03b1\u03b9 \u03c5\u03c0\u03ac\u03c1\u03c7\u03b5\u03b9 \u03c0\u03c1\u03bf\u03ba. \u0398\u03ad\u03bc\u03b1\nvar reasoningId =   ($(\"#reasoning\").getValue().length > 0 ) ? $(\"#reasoning\").getValue(): -1;\nvar reasoningText;\nvar reasoningDDChange = 0;\n\n$(\"#reasoningInsert\").setValue(0); \/\/\u03b1\u03c1\u03c7\u03b9\u03ba\u03ae \u03c4\u03b9\u03bc\u03ae\n\n$(\"#reasoning\").hide();\n$(\"#reasoningDropDown\").hide();\n\n$(\"#SUBJ\").dblclick(function(){\n\n\t$(\"#reasoning\").toggle();\n\t$(\"#reasoningDropDown\").toggle();\n  \n\treasoningId = $(\"#reasoning\").getValue();\n  \treasoningText = $(\"#reasoning\").getText();\n\n});\n\n$(\"#reasoning\").dblclick(function(){\n\n\t$(\"#reasoning\").hide();\n\t$(\"#reasoningDropDown\").hide();\n  \n\t \n});\n\n\n\/\/\n\/\/\n\n$(\"#reasoning\").focusout(function(){\n\n  if( $(\"li.pmdynaform-suggest-list-keyboard\").length == 0 ){\n    \n     if( $(\"#reasoning\").getValue().indexOf($(\"#reasoning\").getText()) === 0 ) {\n   \n     \ti