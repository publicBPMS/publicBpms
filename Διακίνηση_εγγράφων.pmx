$DOC_TYPE = @@DOC_TYPE;
$grpId = @@protScopeGroupUid;

//@@SXETIKA .= (strpos(@@SXETIKA, @@protIncStr ) === false) ? ", ".@@protIncStr : ""  ;

$sxet = preg_split("/[ |,|.|;|\\n|\\r|\\0|\\x0B]/i", @@SXETIKA, -1, PREG_SPLIT_NO_EMPTY);



// Ενημέρωση του πινακα SXETIKA	
if( is_array($sxet) and count($sxet) > 0) {
	
	$dateError = "";
	$sxetError = "";
	
	foreach ($sxet as $i => $aSxet ) {
		list($genProt , $yearProt ) = explode("/", $aSxet);
		$cYear2 = date("y");
		$cYear4 = date("Y");
		$yearProt = (int) $yearProt ;
		
		if( !(( $yearProt >= 11 and $yearProt <= $cYear2 ) or ($yearProt >= 2011 and $yearProt <= $cYear4))){
			$dateError = true;
			$sxetError .= " ".$aSxet;
		} 
		else { //YEAR is OK, hold correct records
			if($yearProt <= $cYear2 ){
				$yearProt = 2000 + $yearProt;
			}
			$sxetOK[] = array('prot' => $genProt , 'year' => $yearProt);
			
		}
	}//foreach
	
	if($dateError) {
		// return to form
		$caseId = @@APPLICATION;
		$indx = @@INDEX;
		G::header("Location: cases_Open?APP_UID=$caseId&DEL_INDEX=$indx&action=draft");
		G::SendMessageText("Το έτος δεν προσδιορίζεται σωστά: ".$sxetError, 'WARNING' );
		die();
	}
	else { //  
		$aWhereClause = array();
		$sxWhereClause = array();
		
		if((@@DOC_TYPE == 2 or @@DOC_TYPE == 4) and !empty(@%PROT_ID) ) {
			$aWhereClause[]= "( PROT_ID =". @%PROT_ID . " AND  YEAR = ". @%YEAR .")" ;
			$sxWhereClause[]= "( SX_PROT_ID =". @%PROT_ID . " AND  SX_YEAR = ". @%YEAR .")" ;
		}
		
		foreach($sxetOK as $i => $pRec ) { //make where clause
			$aWhereClause[]= "( PROT_ID =". $pRec['prot']. " AND YEAR = ". $pRec['year'] .")";
			$sxWhereClause[]= "( SX_PROT_ID =". $pRec['prot']. " AND SX_YEAR = ". $pRec['year'] .")";
		}
		
		$whereClause = implode(' OR ', $aWhereClause );
		$sxetWhereClause = implode(' OR ', $sxWhereClause );
		
		if(@%docHasReg == 1 and is_array(@=rcpts['bpms'][$grpId]) and count (@=rcpts['bpms'][$grpId]) > 0){			
		foreach(@=rcpts['bpms'][$grpId] as $aMan => $dataMan ){
			if ($dataMan['rcptType'] == "TO" ){
				$PROT_ID = $dataMan['protId'];
				$YEAR = @@YEAR;
				
				$qry = "INSERT INTO PMT_SXETIKA(SX_P_CASE,SX_YEAR, SX_PROT_ID,SX_DOC_TYPE, SX_P_CASE_REL,SX_YEAR_REL, SX_PROT_ID_REL, SX_DOC_TYPE_REL) 
				SELECT  DISTINCT '$P_CASE' AS P_CASE,'$YEAR' as YEAR, '$PROT_ID' AS PROT_ID, '$DOC_TYPE' AS DOC_TYPE,  P_CASE AS SX_P_CASE_REL  ,YEAR AS SX_YEAR_REL, PROT_ID AS SX_PROT_ID_REL,  DOC_TYPE AS SX_DOC_TYPE_REL 
				FROM PMT_PROTOKOLLO_PAMTH 
				LEFT JOIN PMT_SXETIKA ON YEAR = SX_YEAR_REL AND PROT_ID = SX_PROT_ID_REL
				WHERE  $sxetWhereClause 
				ORDER BY SX_YEAR_REL,SX_PROT_ID_REL,SX_DOC_TYPE_REL  ";


				executeQuery($qry);
 
			}
		
		}
			
// επιλογή των σχετικών
	$qry = "SELECT APP_DOC_UID, YEAR,PROT_ID,DOC_TYPE,A_NAME, DOC_ID,SUBJ
			FROM (SELECT P_CASE, YEAR,PROT_ID,DOC_TYPE,A_NAME, DOC_ID,SUBJ  
			FROM PMT_PROTOKOLLO_PAMTH   
			LEFT JOIN PMT_AUTHORITIES  ON  SENDER =  A_ID 
			WHERE  $whereClause
			UNION 
			 SELECT P_CASE,YEAR,PROT_ID,DOC_TYPE,A_NAME, DOC_ID,SUBJ 
			FROM  PMT_PROTOKOLLO_PAMTH   
			LEFT JOIN PMT_SXETIKA ON  YEAR = SX_YEAR_REL AND  PROT_ID = SX_PROT_ID_REL
			LEFT JOIN PMT_AUTHORITIES ON  SENDER =  A_ID
			WHERE $sxetWhereClause ) S
			LEFT JOIN APP_DOCUMENT  ON P_CASE =  APP_UID 
			WHERE IF(APP_UID != '', APP_DOC_FIELDNAME like 'doc%', 1) 
			ORDER BY YEAR, PROT_ID, DOC_TYPE";
			
		$sxtFiles = executeQuery($qry);	
 
		} //if docHasReg
		else { // δεν έχει γίνει ακόμη ενημέρρωση του πίνακα SXETIKA
			
