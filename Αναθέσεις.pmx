	$result = executeQuery("SELECT APP_UID FROM APP_DELEGATION WHERE PRO_UID = '$processId' AND USR_UID='$userId' AND DEL_THREAD_STATUS = 'OPEN' AND TAS_UID = '$taskId'");
 
