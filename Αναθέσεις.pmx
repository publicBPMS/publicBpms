if(count(@=chgrpAssignBpmsTo) >0 ){
	foreach(@=chgrpAssignBpmsTo as $i => $u){
	  
		$aUser = PMFInformationUser($u);

		
		@=assignee[$u] = array(
						'userName' => $aUser['lastname'] . " " .$aUser['firstname'],
						'depName' => $aUser['departmentname'],
						'depUid' => $aUser['department'],
						'assignType' => "TO"
									);
				
		$newRowTo .= "<tr>";
		$newRowTo .= "<td>". $aUser['lastname'] . " " .$aUser['firstname'] ."</td>";
		$newRowTo .= "<td>". $aUser['departmentname'] ."</td>";
		$newRowTo .= "<td>". $aUser['position'] ."</td>";
