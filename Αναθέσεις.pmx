        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[#v.4.0 16/7/2017
#  Αναθέσεις χωρίς διαδικασία πρωτοκόλλου(Προϊστέμενοι, Εισηγητές)
#δρομολόγιση με newCase  
#
# Script TASK 'likes' to being run as prevUserUid user
$pU =@@prevUserUid;
$app = @@APPLICATION;
$indx = @@INDEX;

//executeQuery("UPDATE APP_CACHE_VIEW SET USR_UID = '$pU' WHERE APP_UID='$app' AND DEL_INDEX = '$indx'");
#



$pF = @=protFields;
extract($pF);
$prev_PROT_ID_STR = $PROT_ID_STR;
//
//
if(is_array(@=assignee) and count(@=assignee) > 0) { // υπάρχουν αναθέσεις
	
	foreach(@=assignee as $aUser => $aValue) {
 	

//
// protokollo compact gia κάθε παραλήπτη DEPlEVEL <5 έχει χρεώσει με πρωτόκολλο
		if(@%depLevel < 5 ) {
						 	
			$PROT_ID =		$aValue['protId'] ;
			$PROT_ID_DEP = 	$aValue['protIdDep'] ;
			$PROT_DATE =	$aValue['protDate'] ;
			$DEP_UID =		$aValue['depUid'] ;
	
			$PROT_ID_STR = $PROT_ID ."/".$PROT_ID_DEP ."  ". $PROT_DATE;
		
			$locationVars = array('ADA','ADA_DATE','CANCELLED','CLOSE_DATE','CLOSE_RSN','CLOSER','COMMENTS','DEP_MAN','DEP_UID','DOC_CLASS','DOC_DATE','DOC_DLVRY','DOC_ID','DOC_TYPE','DOC_YEAR','EDITOR','EDITOR_DEP','FILE_CAB','GRADING','OPENER','OPENER_DEP','OTHER','P_CASE','PRIORITY','PROT_DATE','PROT_ID','PROT_ID_DEP','PROT_REG_DATE','RCPT_DATE','SCOPE','SEND_DATE','SENDER','SIGN_CASE','STATUS','SUBJ','SUBJ_THEME','SXETIKA','YEAR','CLOSER_NAME','DEP_NAME','DOC_CLASS_STR','DOC_TYPE_STR','DOC_DLVRY_STR','EDITOR_NAME','EDITOR_DEP_NAME','OPENER_NAME','OPENER_DEP_NAME','PROT_ID_STR','SENDER_NAME','STATUS_STR','SUBJ_THEME_STR');

			@=protFields = compact($locationVars);
		
		
			$findTd = "<td id=prot>".$prev_PROT_ID_STR. "<";
 			$replTd = "<td id=prot>".$PROT_ID_STR."<";
 			@@panelDocFiles = str_replace($findTd, $replTd, @@panelDocFiles);
		
		} //depLevel <5
	
 		@=docFiles[1]['prot'] = $PROT_ID_STR;
		
		$aData = array(
	 					'protFields'		=>	@=protFields,
						'docHasReg'			=>	@@docHasReg,
	 					'docFiles'			=>	@=docFiles,
						'panelSxtFiles'		=>	@@panelSxtFiles,
						'panelDocFiles'		=>	@@panelDocFiles,
						'panelCurAssign'	=>	@@panelAssignee,
						'kindOfDocs'		=>	@@kindOfDocs,
						'curAssign'			=>	@=assignee,
						'assignType'		=>	$aValue['assignType'],
						'caseTitle'	 		=>	@@caseTitle,
	 					'labelTitle'		=>	@@labelTitle,
						'nextUserToBeAssigned' => $aUser
	 				  );


		
		
		$newCaseId = PMFNewCaseImpersonate(@@PROCESS,$aUser,$aData );
		
		if($newCaseId){
			
			PMFSendVariables($newCaseId, $aData);
			
			$nc = new Cases();
			$aCase = $nc->loadCase($newCaseId);
			$aCase['APP_STATUS'] = 'TO_DO';
			$nc->updateCase($newCaseId, $aCase);
			
			$prevUser = @@USER_LOGGED;
			executeQuery("UPDATE APP_CACHE_VIEW SET PREVIOUS_USR_UID = '$prevUser' 
						  WHERE APP_UID = '$newCaseId' AND DEL_INDEX = 1 ");
			
			executeQuery("UPDATE APP_DELEGATION SET USR_UID = '$prevUser' 
						  WHERE APP_UID = '$app' AND DEL_INDEX = 2 ");
			
			// remove users to be derived correctly, so 		
			if ( ($key = array_search($aUser,@=chgrpAssignBpmsTo))!== false) {
  				 unset(@=chgrpAssignBpmsTo[$key]) ;
				 
			}
			
			if ( ($key = array_search($aUser,@=chgrpAssignBpmsCc))!== false) {
  				 unset(@=chgrpAssignBpmsCc[$key]) ;
			}
		
		}
	

	}//foreach
	



//υπάρχουν χρήστες που δεν χρεώθηκαν παραπάνω. Νεα προσπάθεια

if(count(@=chgrpAssignBpmsTo) > 0 or count(@=chgrpAssignBpmsCc)>0 ) { 
		
		@@reAssignToRestUsers = 1;
		PMFSendVariables(@@APPLICATION, array('reAssignToRestUsers' => @@reAssignToRestUsers ));
	
		$dOK = PMFDerivateCase(@@APPLICATION, 1);
		$caseId = @@APPLICATION;
		if($dOK == 1 ){
	 
 			G::header("Location: cases_Open?APP_UID=$caseId&DEL_INDEX=1&action=draft");
 
 		} else G::header("Location: casesListExtJsRedirector");
	
		die();
	}
}// υπάρχουν παραλήπτες
]]></tri_webbot>
