        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// v.2.2 11/8/2016
//περιοχή πρωτοκόλλου που ανήκει ο χρήστης η ιεραρχία καθώς και όλη η ιεραρχία

$cUser = @@USER_LOGGED;

$usrInf = PMFInformationUser($cUser);

@@EDITOR_STR = $usrInf['lastname']." ".$usrInf['firstname'];
@@EDITOR = @@USER_LOGGED;
@@USR_FULLNAME = $usrInf['lastname'] ." ".$usrInf['firstname'];
@@DEP_UID = $usrInf['department'];
@@DEP_NAME = $usrInf['departmentname'];

$depUid = @@DEP_UID;

$result = executeQuery("SELECT S.P_SCOPE, C.CON_ID, S.DEP_LEVEL FROM PMT_PROTOKOLLO_SCOPE S, CONTENT C WHERE  S.DEP_UID = '$depUid' and S.P_SCOPE =C.CON_VALUE  AND C.CON_LANG = 'el' ");

@@SCOPE = $result[1]['P_SCOPE'];
@@protScopeGroupUid = $result[1]['CON_ID'];
@@depLevel = $result[1]['DEP_LEVEL'];


if(($usrInf['reportsto'] == "" or $usrInf['reportsto'] === NULL )and $usrInf['departmentname'] != "Περιφερειάρχης") {
		G::SendMessageText("DEN KATAXORITHIKATE SE KAPOIO TMHMA\nH DIADIKASIA DEN MPOREI NA SYNEXISTEI \nPARAKALO EPIKOINONHSTE ME TON DIAXEIRISTH", "WARNING");
		print "Δεν καταχωρηθήκατε σε κάποιο Τμήμα\nΗ διαδικασία δεν μπορεί να συνεχιστεί \nΠαρακαλώ επικοινωνήστε με τον Διαχειριστή";
		$caseId = @APPLICATION;
		$index = @@INDEX;
		G::header("Location: cases_Open?APP_UID=$caseId&DEL_INDEX=$index");
		die();
}
else {
	$userMan = executeQuery("SELECT DEP_MANAGER FROM DEPARTMENT WHERE DEP_MANAGER = '$cUser'"); // is editor a manager ?
	if(@@depLevel == 7 and (!is_array($userMan) or count($userMan)== 0 )) {
		@@depLevel += 1; //υπάλληλος τμήματος
	}
	
	$depLev = @@depLevel;
	$editorMan = array();
	$depId = $usrInf['department'];

		do{ // όλη η απο πάνω ιεραρχία μαζί με τον χρήστη
	
			
			$userName = $usrInf['lastname'] ." ".$usrInf['firstname'];

   			$editorMan[$depLev] =  array('userUid'  => $cUser,
										 'depUid'  => $depId,
										 'userName' => $userName,
										 'depName'  => $usrInf['departmentname'],
										 'depLevel' => $depLev,
										 'icon' => "",
						  				 'signStat' => "",
						     			 'signDate' => "",
						     			 'comments' => ""
							 			);

			$cUser = $usrInf['reportsto'];
			$usrInf = PMFInformationUser($cUser);
			$depId = $usrInf['department'];
			$result = executeQuery("SELECT DEP_LEVEL FROM PMT_PROTOKOLLO_SCOPE WHERE DEP_UID = '$depId'");
			$depLev = $result[1]['DEP_LEVEL'];
		} while ($cUser != "" or $cUser != NULL );


// προσθέτουμε και τον αντιπεριφερειάρχη..... Ποιον απο όλους ?
//Θέλει μάλλον έναν πίνακα που να ορίζονται οι σχέσεις ( πιθανόν στο scope)
//προς το παρόν κρατώ μόνο τους αντιπεριφερειάρχες ΠΕ
//o τίτλος  να είναι μοναδικά ορισμένο στο LIKE
// Η ύπόθεση είναι ότι οι θεματικοί Αντιπεριφερειάρχες δεν συγκρούονται ιεραρχικά με τους ΠΕ
	
$scope = @@SCOPE;
if (@@depLevel >= 5) { 
	$aInfo = executeQuery("SELECT DEP_MANAGER FROM PMT_PROTOKOLLO_SCOPE S, DEPARTMENT D WHERE S.P_SCOPE = '$scope' AND S.DEP_UID = D.DEP_UID AND (S.DEP_NAME LIKE 'Αντιπεριφερειάρχης ΠΕ%' or S.DEP_NAME LIKE 'ΑΝΤΙΠΕΡΙΦΕΡΕΙΑΡΧΗΣ ΠΕ%')");
	if (is_array($aInfo) or count($aInfo) >0) {
		$usrInf = PMFInformationUser($aInfo[1]['DEP_MANAGER']);
		$userName = $usrInf['lastname'] ." ".$usrInf['firstname'];
$editorMan[2] =  array('userUid'  => $aInfo[1]['DEP_MANAGER'],
					 'depUid'  => $usrInf['department'],
					 'userName' => $userName,
					 'depName'  => $usrInf['departmentname'],
					 'depLevel' => 2,
					 'icon' => "",
					 'signStat' => "",
					 'signDate' => "",
					 'comments' => ""
					);

	}
}

	ksort($editorMan);
	@=docEditorManagers = $editorMan;

	
}
]]></tri_webbot>
