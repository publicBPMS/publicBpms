move();\n//-------------\n\n\n//\n$(\"#currentUserAssigned\").hide();\n\nshowHide(\"2\");\n\n//\n//\n//if CC. Δεν υπάρχει ενέργεια, μόνο ενημέρωση\n//\nif($(\"#assignType\").getValue() == \"CC\") {\n \n  $(\".pmdynaform-control-radio[value='2']\").attr(\"disabled\", \"\");\n  $(\".pmdynaform-control-radio[value='3']\").next().html(\"Ενημερώθηκα\");\n  $(\"#assignment\").setValue(3);\n  $(\"#STATUS\").hide();\n  $(\"#CLOSE_RSN\").hide();\n  showHide(\"3\");\n}\n//\n//\n\n\n$(\"#assignment\").setOnchange(function(){\n  var aVal = $(\"#assignment\").getValue();\n  showHide(aVal);\n  \n });\n  \n function showHide(s){\n    \n    switch(s) {\n    case \"2\":\n        $(\"#STATUS\").hide();\n  \t\t$(\"#CLOSE_RSN\").hide();\n        $(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n        $(\"#COMMENTS\").show();\n        break;\n    case \"3\":\n     \n     \tif($(\"#assignType\").getValue() == \"TO\") {\n       \t\t$(\"#STATUS\").show();\n   \t\t\t$(\"#CLOSE_RSN\").show();\n          \t//$(\"#COMMENTS\").hide();\n     \t}\n        break;\n    \n\t}\n  \n   $(\"#submit1\").setValue($(\".pmdynaform-control-radio[value='\"+s+\"']\").next().html());\n  $(\"#form\\\\[submit1\\\\]\").css(\"background-color\",\"#DAF7A6\"); \n}\n\n\n//\n// COMMENTS management\n\n$(\"#form\\\\[COMMENTS\\\\]\").click(function(){\n  $(\"#form\\\\[COMMENTS\\\\]\").attr(\"rows\", \"6\");\n});\n\n$(\"#form\\\\[COMMENTS\\\\]\").focusout(function(){\n  $(\"#form\\\\[COMMENTS\\\\]\").attr(\"rows\", \"2\");\n});\n\n \n//\n\n     \n\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title1","label":" Εκτέλεση εργασίας εισηγητή","colSpan":12}],[{"type":"textarea","variable":"SUBJ","var_uid":"81130981457ab42b6c29611041528511","dataType":"string","protectedValue":false,"id":"SUBJ","name":"SUBJ","label":"Θέμαa","defaultValue":"","placeholder":"","hint":"","required":false,"validate":"","validateMessage":"","