        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[//v.2.1 12/8/2016
//  Αναθέσεις χωρίς διαδικασία πρωτοκόλλου(Προϊστέμενοι, Εισηγητές)
//δρομολόγιση με newCase  
#
if(!isset(@@onceSend) or empty(@@onceSend)) { //δημιουργία πίνακα παραληπτών απαξ
	@@onceSend = "OK";	
	if(current(@=assign) !== false ){ // τουλάχιστον υπάρχει παραλήπτης 
		@=tmpAssign = @=assign ; // ta Uid των ομάδων των γραμματειών
	}
}
@@moreAssign = 0;
if(is_array(@=tmpAssign) and count(@=tmpAssign) > 0) { // υπάρχουν παραλήπτες
	@@moreAssign = 1;
	$nextUserToBeAssigned = key(@=tmpAssign);

	$processId = '73709106457ab42b234f580083810458'; //Αναθέσεις
	$dummyTaskId = '78918457057ab62a0cff575031000968'; //dummy task παραλαβής εσωτερικών εισερχομένων για πρωτοκόλληση 
	$noUser = '86636352557a5da0d6db678062448546'; //bpmssystem System as dummy user
	
	$client = new SoapClient('http://' . $_SERVER['HTTP_HOST'] . '/sys' . @@SYS_SYS . '/en/neoclassic/services/wsdl2');
	$params = array(array('userid'=>'bpmssystem', 'password'=>'md5:e01bb1c7a5094ee00ea82c2fc799a040'));
	
	$result = $client->__SoapCall('login', $params);
	if ($result->status_code == 0) 
		$sessionId = $result->message;
	else 
  		G::SendMessageText( "Unable to connect to ProcessMaker.\nError Number: $result->status_code\n" . "Error Message: $result->message\n", 'ERROR');

	class variableStruct {
   		public $name;
   		public $value;
	}
	$name = new variableStruct();
	$vars = array('nextUserToBeAssigned' => $nextUserToBeAssigned,
			'kindOfDocs' => @@kindOfDocs,
			'srcpts' => serialize(@=rcpts),
			'panelRcpts' => @@panelRcpts,
			'sprotFields' => serialize(@=protFields),
			'smainFolder' => serialize(@@mainFolder),
			'caseTitle' => @@caseTitle,
			'prevUserUid' => @@USER_LOGGED,
			'panelDocFiles' => @@panelDocFiles
			 );
	$aVars = array();
	foreach ($vars as $key => $val) { 
   		$obj = new variableStruct();
   		$obj->name = $key;
   		$obj->value = $val;
   		$aVars[] = $obj;      
	}
  
# Use the functions processList() and taskList() to look up the IDs for the 
# process and its first task.
#
	$params = array(array('sessionId'=>$sessionId, 'processId'=>$processId, 
   'taskId'=>$dummyTaskId, 'variables'=>$aVars));
	$result = $client->__SoapCall('newCase', $params);
#
	print "New case ID: $result->caseId, Case No: $result->caseNumber \n";
	if ($result->status_code == 0){
  		print "New case ID: $result->caseId, Case No: $result->caseNumber \n";
		executeQuery("UPDATE APPLICATION SET APP_STATUS='TO_DO' WHERE APP_UID='$result->caseId' AND APP_STATUS='DRAFT'");
		#second derive case to secretary group
		$params = array(array('sessionId'=>$sessionId, 
   								'caseId'=>$result->caseId, 'delIndex'=>'1'));
		$result = $client->__SoapCall('routeCase', $params);
		if ($result->status_code != 0)
   		 	G::SendMessageText( "Error deriving case: $result->message \n", 'ERROR');
		}
	else 
   		G::SendMessageText( "Error creating case: $result->message \n", 'ERROR');

	array_shift(@=tmpAssign);	
	if(is_array(@=tmpAssign) and count(@=tmpAssign) == 0) @@moreAssign = 0;
}# τέλος αρχικού if]]></tri_webbot>
