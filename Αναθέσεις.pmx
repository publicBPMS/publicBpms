        <dyn_content><![CDATA[{"name":"\u0391\u03bd\u03ac\u03b8\u03b5\u03c3\u03b7 \u03b5\u03b9\u03c3\u03b5\u03c1\u03c7\u03bf\u03bc\u03ad\u03bd\u03bf\u03c5 \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03bf\u03c5 \u03c3\u03b5 \u03c5\u03c6\u03b9\u03c3\u03c4\u03b1\u03bc\u03ad\u03bd\u03bf\u03c5\u03c2 2.0","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"20382690457ab480b14a8b8074974699","name":"\u0391\u03bd\u03ac\u03b8\u03b5\u03c3\u03b7 \u03b5\u03b9\u03c3\u03b5\u03c1\u03c7\u03bf\u03bc\u03ad\u03bd\u03bf\u03c5 \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03bf\u03c5 \u03c3\u03b5 \u03c5\u03c6\u03b9\u03c3\u03c4\u03b1\u03bc\u03ad\u03bd\u03bf\u03c5\u03c2 2.0","description":"","mode":"edit","script":{"type":"js","code":"$(\"form\").css(\"background-color\", \"#DEE0EC\");\n\n$(\"#form\\\\[submit1\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[submit2\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[submit3\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\n$(\"#title1\").css(\"color\",\"#ffffff\");\n$(\"#title1\").css(\"background-color\",\"#428bca\");\n\n\n$(\"#Prot\").css(\"background-color\",\"#D6E0F3\");\n\n\n$(\"#panelSxtFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelDocFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelIntDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n$(\"#panelIntDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelSigners\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelAssign\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelInitRcpts\").css(\"background-color\",\"#D8D8C0\");\n\n$(\"#gridAssignTo\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridAssignCc\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridAssignTo\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n$(\"#gridAssignCc\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n\n\/\/\n\/\/\n$(\"#currentUserAssigned\").hide();\n\n\/\/ sxet panel\n\/\/\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) \n    \t$(selector).show();\n  \telse\n    \t$(selector).hide();\n  }\n}\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelAssign\");\npanelDisplay(\"panelDocFiles\");\npanelDisplay(\"panelSxtFiles\");\npanelDisplay(\"panelInitDocFiles\");\npanelDisplay(\"panelInitRcpts\");\n\n\n$(\"#sxtTitle\").hide();\n$(\"#panelInitRcpts\").hide();\n$(\"#panelInitFiles\").hide();\n$(\"#panelSxtFiles\").hide();\n  \n($(\"#xaraktirismos\").getValue() == 1 ) ?  $(\"#PROT_ID_STR\").hide() : $(\"#PROT_ID_STR\").show(); \n \n$(\"#sxtButton\").click(function(){\n    $(\"#sxtTitle\").toggle();\n    $(\"#panelInitRcpts\").toggle();\n    $(\"#panelInitFiles\").toggle();\n    $(\"#panelSxtFiles\").toggle();\n    \n});\n\/*\n*\/\n\n$(\"#STATUS\").hide();\n$(\"#CLOSE_RSN\").hide();\n\n\nfunction selfAssign() {\n  if($(\"#giveAnAnswer\").getValue() == 1) {\n    $(\"#STATUS\").hide();\n\t$(\"#CLOSE_RSN\").hide();\n   \t$(\"#STATUS\").setValue(\"\");\n\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t$(\"#STATUS_label\").setValue(\"\");\n    $(\"#gridAssignTo\").show();\n\t$(\"#gridAssignCc\").show();\n    $(\"#COMMENTS\").show();\n    $(\"#assignment\").show();\n    $(\"#assignment\").setValue(\"\");\n   \n\n   }\n  else if($(\"#giveAnAnswer\").getValue() == 2) {\n       \t$(\"#STATUS\").show();\n   \t\t$(\"#CLOSE_RSN\").show();\n   \t\t$(\"#gridAssignTo\").hide();\n\t\t$(\"#gridAssignCc\").hide();\n    \t$(\"#COMMENTS\").hide();\n    \t$(\"#assignment\").show();\n    \t\n    \t$(\"#assignment\").setValue(\"\");\n\n   } \n   $(\"#giveAnAnswer\").setOnchange(selfAssign);\n}\n\n\/\/\n\/\/\u03ba\u03bb\u03b7\u03c1\u03bf\u03bd\u03bf\u03bc\u03b9\u03ba\u03cc\u03c4\u03b7\u03c4\u03b1 \u03c4\u03bf\u03c5 CC. \u0394\u03b5\u03bd \u03c5\u03c0\u03ac\u03c1\u03c7\u03b5\u03b9 \u03b5\u03bd\u03ad\u03c1\u03b3\u03b5\u03b9\u03b1, \u03bc\u03cc\u03bd\u03bf \u03b5\u03bd\u03b7\u03bc\u03ad\u03c1\u03c9\u03c3\u03b7\n\/\/\nif($(\"#inherAssignType\").getValue() == \"CC\") {\n  $(\".pmdynaform-control-radio[value='1']\").next().html(\"\u039a\u03bf\u03b9\u03bd\u03bf\u03c0\u03bf\u03af\u03b7\u03c3\u03b7 \u03c3\u03b5 \u03c5\u03c6\u03b9\u03c3\u03c4\u03ac\u03bc\u03b5\u03bd\u03bf\u03c5\u03c2\")\n  $(\".pmdynaform-control-radio[value='2']\").attr(\"disabled\", \"\");\n  $(\".pmdynaform-control-radio[value='3']\").next().html(\"\u0395\u03bd\u03b7\u03bc\u03b5\u03c1\u03ce\u03b8\u03b7\u03ba\u03b1\");\n  $(\"#gridAssignTo\").hide();\n}\n\/\/\n\/\/\n\nif($(\"#depLevel\").getValue() >= 6 ) $(\".pmdynaform-control-radio[value='4']\").parent().hide();\n\nselectAssign($(\"#assignment\").getValue());\n\n$(\"#assignment\").setOnchange(function(){\n  var s= $(\"#assignment\").getValue();\n  selectAssign(s);\n});\n\nfunction selectAssign(s){\n  \n  switch(s) {\n    case \"1\":\n       $(\"#STATUS\").hide();\n   \t\t$(\"#CLOSE_RSN\").hide();\n    \t$(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n    \tpanelDisplay(\"panelInitRcpts\");\n    \n       \t$(\"#gridAssignTo\").show();\n      \t$(\"#gridAssignCc\").show();\n    \t$(\"#COMMENTS\").show();\n        break;\n    case \"2\":\n        $(\"#STATUS\").hide();\n   \t\t$(\"#CLOSE_RSN\").hide();\n        $(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n   \t\t$(\"#panelInitRcpts\").hide();\n      \t$(\"#gridAssignTo\").hide();\n      \t$(\"#gridAssignCc\").hide();\n      \t$(\"#COMMENTS\").hide();\n        break;\n    case \"3\": \/\/\u03ba\u03bb\u03b5\u03af\u03c3\u03b9\u03bc\u03bf \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03bf\u03c5\n      \t$(\"#STATUS\").setValue(\"3001\");\n     \n     \tif($(\"#assignType\").getValue() == \"TO\") {\n       \t\t$(\"#STATUS\").show();\n   \t\t\t$(\"#CLOSE_RSN\").show();\n     \t}\n  \t\t\n   \t\t$(\"#panelRcpts\").hide();\n      \t$(\"#gridAssignTo\").hide();\n      \t$(\"#gridAssignCc\").hide();\n     \t$(\"#COMMENTS\").hide();\n        break;\n        \n   case \"4\": \/\/\u03b4\u03b9\u03b1\u03b2\u03af\u03b2\u03b1\u03c3\u03b7 \u03c3\u03b5 \u03ac\u03bb\u03bb\u03b7 \u03c5\u03c0\u03b7\u03c1\u03b5\u03c3\u03af\u03b1\n        $(\"#STATUS\").hide();\n   \t\t$(\"#CLOSE_RSN\").hide();\n        $(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n   \t\t$(\"#panelInitRcpts\").hide();\n      \t$(\"#gridAssignTo\").hide();\n      \t$(\"#gridAssignCc\").hide();\n      \t$(\"#COMMENTS\").hide();\n        break;\n        \n    default:\n        \n\t}\n \n}\n\n$(\"#gridAssignTo\").onAddRow(function(){\n  $(\"#gridAssignTo\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n});\n$(\"#gridAssignCc\").onAddRow(function(){\n  $(\"#gridAssignCc\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n});\n\n\n\/\/correcting grid-dropdown problems\n\/\/\nif($(\"#assignJSON\").getValue().length != 0 ) {\n\nvar assign = JSON.parse($(\"#assignJSON\").getValue());\nvar nRcpts = Object.keys(assign).length;\n\n\/\/for TO\nvar assignTo = JSON.parse($(\"#assignToJSON\").getValue());\nvar gridId = $(\"#gridAssignTo\");\nvar r = $(\"#gridAssignTo\").getNumberRows();\nfor (var i = 1 ; i <= r ; i++) {\n  if (typeof assignTo[i] != 'undefined' && assignTo[i]['userAsTo'] != '') {\n    var selector = \"select#form\\\\[gridAssignTo\\\\]\\\\[\" + i + \"\\\\]\\\\[userAsTo\\\\]\";\n\n    \n    \n    for (var j = 1; j <= nRcpts ; j++) {\n        if( assign[j]['USR_UID'] == assignTo[i]['userAsTo'] ) {\n          $(selector).append('<option selected=\"selected\" value=\"' + assign[j]['USR_UID'] + '\">' + assign[j]['user'] + '<\/option>');\n        }\n        else {\n          $(selector).append('<option value=\"' + assign[j]['USR_UID'] 