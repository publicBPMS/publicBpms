			$resp = executeQuery("SELECT CONTACT_ID FROM PMT_CONTACT WHERE CONTACT_NAME = '$CONTACT_NAME'");
			
			$CONTACT_ID = $resp[1];
			@=gridOtherRcpts[$key]['cNameId'] = $CONTACT_ID;
