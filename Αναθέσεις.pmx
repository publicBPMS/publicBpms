		$newRowCc .= "<td>". $aUser['departmentname'] ."</td>";
		$newRowCc .= "<td>". $aUser['position'] ."</td>";
		$newRowCc .= "</tr>";
			 
			
	}
}



$panelHead = "
<b> ΠΡΟΣ ενέργεια: </b> <br>
<table style=\"width:100%\">
  <tr>
    <th>Όνομα</th>
	<th>Τμήμα</th>
	<th>θέση</th>
   </tr> ";

$panelCc =  "<tr assigneeCCPart><td><br><b> ΚΟΙΝΟΠΟΙΗΣΗ: </b>";


@@panelAssignee = @@panelCurAssignee;
	
if(strlen(@@panelAssignee) >10 ){
		
	if(preg_match('/<tr assigneeCCPart>/', @@panelAssignee) == 1){
		
		@@panelAssignee = preg_replace('/<tr assigneeCCPart>/',$newRowTo."<tr assigneeCCPart>",  @@panelAssignee);
			
		@@panelAssignee = preg_replace('/<\/table>/',$newRowCc."</table>",  @@panelAssignee);
			
	} else {
		
		@@panelAssignee = preg_replace('/<\/table>/',$newRowTo."</table>",  @@panelAssignee);
