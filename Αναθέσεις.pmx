        <dyn_content><![CDATA[{"name":"\u039a\u03bb\u03b5\u03af\u03c3\u03b9\u03bc\u03bf \u03c0\u03c1\u03c9\u03c4\u03bf\u03ba\u03cc\u03bb\u03bb\u03bf\u03c5 2.0","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"312652587582f46c08c5fc2060506052","name":"\u039a\u03bb\u03b5\u03af\u03c3\u03b9\u03bc\u03bf \u03c0\u03c1\u03c9\u03c4\u03bf\u03ba\u03cc\u03bb\u03bb\u03bf\u03c5 2.0","description":"","mode":"edit","script":{"type":"js","code":"$(\"form\").css(\"background-color\", \"#DEE0EC\");\n\n$(\"#form\\\\[submit1\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[submit2\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[submit3\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\n$(\"#title1\").css(\"color\",\"#ffffff\");\n$(\"#title1\").css(\"background-color\",\"#428bca\");\n\n\n$(\"#Prot\").css(\"background-color\",\"#D6E0F3\");\n\n\n$(\"#panelSxtFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelDocFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelIncDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n\n\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelSigners\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelAssign\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelIncRcpts\").css(\"background-color\",\"#D8D8C0\");\n\n\/\/$(\"#gridAssignTo\").css(\"background-color\",\"#D8D8C0\");\n\/\/$(\"#gridAssignCc\").css(\"background-color\",\"#D8D8C0\");\n\/\/$(\"#gridAssignTo\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n\/\/$(\"#gridAssignCc\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n\n$(\".pmdynaform-control-checkbox-list\").css(\"background-color\",\"#D8D8C0\");\n\n\/\/\n\/\/\n$(\"#currentUserAssigned\").hide();\n\n\/\/ sxet panel\n\/\/\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) {\n    \t$(selector).show();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) {\n        $(\"#sxetikaButton\").show();\n        $(selector).hide();\n      }\n    }\n    else {\n    \t$(selector).hide();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) $(\"#sxetikaButton\").hide();\n    }\n  }\n}\n\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelAssign\");\npanelDisplay(\"panelDocFiles\");\npanelDisplay(\"panelSxtFiles\");\n \n \n  \n($(\"#PROT_ID_STR\").getValue().length == 0 ) ?  $(\"#PROT_ID_STR\").hide() : $(\"#PROT_ID_STR\").show(); \n \n$(\"#sxetikaButton\").click(function(){\n     \n    $(\"#panelSxtFiles\").toggle();\n    \n});\n\n $(\"a[href*='localhost:34300']\").remove(); \/\/remove links to localhost\n\n\/*\n*\/\n\n$(\"#STATUS\").hide();\n$(\"#CLOSE_RSN\").hide();\n\n\n\/\/\n\/\/if CC. \u0394\u03b5\u03bd \u03c5\u03c0\u03ac\u03c1\u03c7\u03b5\u03b9 \u03b5\u03bd\u03ad\u03c1\u03b3\u03b5\u03b9\u03b1, \u03bc\u03cc\u03bd\u03bf \u03b5\u03bd\u03b7\u03bc\u03ad\u03c1\u03c9\u03c3\u03b7\n\/\/\nif($(\"#assignType\").getValue() == \"CC\") {\n  $(\".pmdynaform-control-radio[value='1']\").next().html(\"\u039a\u03bf\u03b9\u03bd\u03bf\u03c0\u03bf\u03af\u03b7\u03c3\u03b7 \u03c3\u03b5 \u03c5\u03c6\u03b9\u03c3\u03c4\u03ac\u03bc\u03b5\u03bd\u03bf\u03c5\u03c2\")\n  $(\".pmdynaform-control-radio[value='2']\").attr(\"disabled\", \"\");\n  $(\".pmdynaform-control-radio[value='3']\").next().html(\"\u0395\u03bd\u03b7\u03bc\u03b5\u03c1\u03ce\u03b8\u03b7\u03ba\u03b1\");\n  $(\"#chgrpAssignBpmsTo\").hide();\n}\n\/\/\n\/\/\n\n\nif($(\"#depLevel\").getValue() == 4 || $(\"#depLevel\").getValue() == 5) {\n  $(\".pmdynaform-control-radio[value='4']\").parent().show();\n}\nelse {\n  $(\".pmdynaform-control-radio[value='4']\").parent().hide();\n}\n\nvar s= $(\"#assignment\").getValue();\nselectAssign(s);\n\n$(\"#assignment\").setOnchange(function(){\n  var s= $(\"#assignment\").getValue();\n  selectAssign(s);\n  \n});\n\nfunction selectAssign(s){\n  \n  switch(s) {\n    case \"1\":\n       $(\"#STATUS\").hide();\n   \t\t$(\"#CLOSE_RSN\").hide();\n    \t$(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n      \n      \tif($(\"#depLevel\").getValue() <= 5){\n        \tpanelDisplay(\"panelAssign\");\n          \tpanelDisplay(\"panelIncRcpts\");\n      \t}\n      \telse {\n        \t$(\"#panelAssign\").show() ;\n          \t$(\"#panelIncRcpts\").hide() ;\n      \t}\n            \n    \n       \tif($(\"#assignType\").getValue() == \"TO\") $(\"#chgrpAssignBpmsTo\").show();\n      \t$(\"#chgrpAssignBpmsCc\").show();\n    \t$(\"#COMMENTS_NEW\").show();\n      ($(\"#COMMENTS\").getValue().length >3)  ? $(\"#COMMENTS\").show() :$(\"#COMMENTS\").hide();\n        break;\n    case \"2\":\n        $(\"#STATUS\").hide();\n   \t\t$(\"#CLOSE_RSN\").hide();\n        $(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n   \t\t$(\"#panelRcpts\").hide();\n        $(\"#panelAssign\").hide();\n      \t$(\"#chgrpAssignBpmsTo\").hide();\n      \t$(\"#chgrpAssignBpmsCc\").hide();\n      \t$(\"#COMMENTS\").hide();\n        $(\"#COMMENTS_NEW\").hide();\n        break;\n    case \"3\": \/\/\u03ba\u03bb\u03b5\u03af\u03c3\u03b9\u03bc\u03bf \u03b5\u03b3\u03b3\u03c1\u03ac\u03c6\u03bf\u03c5\n      \t$(\"#STATUS\").setValue(\"3001\");\n     \n     \tif($(\"#assignType\").getValue() == \"TO\") {\n       \t\t$(\"#STATUS\").show();\n   \t\t\t$(\"#CLOSE_RSN\").show();\n     \t}\n  \t\t\n   \t\t$(\"#panelRcpts\").hide();\n        $(\"#panelAssign\").hide();\n      \t$(\"#chgrpAssignBpmsTo\").hide();\n      \t$(\"#chgrpAssignBpmsCc\").hide();\n     \t$(\"#COMMENTS\").hide();\n      \t$(\"#COMMENTS_NEW\").hide();\n        break;\n        \n   case \"4\": \/\/\u03b4\u03b9\u03b1\u03b2\u03af\u03b2\u03b1\u03c3\u03b7 \u03c3\u03b5 \u03ac\u03bb\u03bb\u03b7 \u03c5\u03c0\u03b7\u03c1\u03b5\u03c3\u03af\u03b1\n        $