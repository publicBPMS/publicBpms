        <dyn_content><![CDATA[{"name":"Ανάθεσεις Προϊσταμένων2.0","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"74468994157dd66a09985f7008651602","name":"Ανάθεσεις Προϊσταμένων2.0","description":"","mode":"edit","script":{"type":"js","code":"$(\"form\").css(\"background-color\", \"#DEE0EC\");\n\n\n$(\".form-control\").css(\"font-size\", \"0.9em\");\n$(\".textlabel\").css(\"font-size\", \"0.88em\");\n$(\".title-column\").css(\"font-size\", \"0.88em\");\n$(\".pmdynaform-grid-title\").css(\"font-size\", \"0.9em\");\n\n$(\"#form\\\\[submit1\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[submit2\\\\]\").css(\"background-color\",\"#DAF7A6\");\n$(\"#form\\\\[submit3\\\\]\").css(\"background-color\",\"#DAF7A6\");\n\n$(\"#title1\").css(\"color\",\"#ffffff\");\n$(\"#title1\").css(\"background-color\",\"#428bca\");\n\n\n$(\"#Prot\").css(\"background-color\",\"#D6E0F3\");\n\n\n$(\"#panelSxtFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelDocFiles\").css(\"background-color\",\"#CCD8CC\");\n$(\"#panelIncDocFiles\").css(\"background-color\",\"#CCD8CC\");\n\n\n\n$(\"#panelRcpts\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelSigners\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelAssignee\").css(\"background-color\",\"#D8D8C0\");\n$(\"#panelIncRcpts\").css(\"background-color\",\"#D8D8C0\");\n\n//$(\"#gridAssignTo\").css(\"background-color\",\"#D8D8C0\");\n//$(\"#gridAssignCc\").css(\"background-color\",\"#D8D8C0\");\n//$(\"#gridAssignTo\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n//$(\"#gridAssignCc\").find(\".row\").css(\"background-color\",\"#D8D8C0\");\n\n$(\".pmdynaform-control-checkbox-list\").css(\"background-color\",\"#D8D8C0\");\n\n//\n//\n$(\"#currentUserAssigned\").hide();\n\n// sxet panel\n//\nfunction panelDisplay(elId) {\n  var selector =  \"#\"+elId;\n  var searchEl = \"@@\"+elId;\n  \n  if($(selector).find(\".panel-body\").length > 0 ) {\n  \tif( $(selector).find(\".panel-body\").html().indexOf(searchEl) == -1 && $(selector).find(\".panel-body\").html().length >0) {\n    \t$(selector).show();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) {\n        $(\"#sxetikaButton\").show();\n        $(selector).hide();\n      }\n    }\n    else {\n    \t$(selector).hide();\n      if( elId.indexOf(\"SxtFiles\") > 0 ) $(\"#sxetikaButton\").hide();\n    }\n  }\n}\n\n\npanelDisplay(\"panelRcpts\");\npanelDisplay(\"panelAssignee\");\npanelDisplay(\"panelDocFiles\");\npanelDisplay(\"panelSxtFiles\");\npanelDisplay(\"panelIncRcpts\");\n\n//-------------\n\n$(\".viewLCDocument\").remove();\n$(\".editDocument\").remove();\n$(\".signDocument\").remove();\n$(\".validDocument\").remove();\n$(\".uploadNewVersion\").remove();\n//-------------\n\n\n(!$(\"#labelTitle\").getValue() ) ?  '' : $(\"#title1\").find('span.textlabel').html($(\"#labelTitle\").getValue()); \n  \n($(\"#PROT_ID_STR\").getValue().length == 0 ) ?  $(\"#PROT_ID_STR\").hide() : $(\"#PROT_ID_STR\").show(); \n \n$(\"#sxetikaButton\").click(function(){\n     \n    $(\"#panelSxtFiles\").toggle();\n    \n});\n\n \n/*\n*/\n\n$(\"#STATUS\").hide();\n$(\"#CLOSE_RSN\").hide();\n\n\n//\n//if CC. Δεν υπάρχει ενέργεια, μόνο ενημέρωση\n//\nif($(\"#assignType\").getValue() == \"CC\") {\n  $(\".pmdynaform-control-radio[value='1']\").next().html(\"Κοινοποίηση σε υφιστάμενους\")\n  $(\".pmdynaform-control-radio[value='2']\").attr(\"disabled\", \"\");\n  $(\".pmdynaform-control-radio[value='3']\").next().html(\"Ενημερώθηκα\");\n  $(\"#chgrpAssignBpmsTo\").hide();\n}\n//\n//\n\n\nif($(\"#depLevel\").getValue() == 4 || $(\"#depLevel\").getValue() == 5) {\n  $(\".pmdynaform-control-radio[value='4']\").parent().show();\n}\nelse {\n  $(\".pmdynaform-control-radio[value='4']\").parent().hide();\n}\n\n\n\n$(\"#assignment\").setOnchange(function(){\n  var s= $(\"#assignment\").getValue();\n  selectAssign(s);\n  \n});\n\nfunction selectAssign(s){\n  \n  switch(s) {\n    case \"1\":\n       $(\"#STATUS\").hide();\n   \t\t$(\"#CLOSE_RSN\").hide();\n    \t$(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n      \n      \tif($(\"#depLevel\").getValue() <= 5){\n        \tpanelDisplay(\"panelAssignee\");\n          \tpanelDisplay(\"panelIncRcpts\");\n      \t}\n      \telse {\n        \t$(\"#panelAssignee\").show() ;\n          \t$(\"#panelIncRcpts\").hide() ;\n      \t}\n            \n    \n       \tif($(\"#assignType\").getValue() == \"TO\") $(\"#chgrpAssignBpmsTo\").show();\n      \t$(\"#chgrpAssignBpmsCc\").show();\n    \t$(\"#COMMENTS_NEW\").show();\n      ($(\"#COMMENTS\").getValue().length >3)  ? $(\"#COMMENTS\").show() :$(\"#COMMENTS\").hide();\n        break;\n    case \"2\":\n        $(\"#STATUS\").hide();\n   \t\t$(\"#CLOSE_RSN\").hide();\n        $(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n   \t\t$(\"#panelRcpts\").hide();\n        $(\"#panelAssignee\").hide();\n      \t$(\"#chgrpAssignBpmsTo\").hide();\n      \t$(\"#chgrpAssignBpmsCc\").hide();\n      \t$(\"#COMMENTS\").hide();\n        $(\"#COMMENTS_NEW\").hide();\n        break;\n    case \"3\": //κλείσιμο εγγράφου\n    //  (!$(\"#STATUS\").getValue()) ? $(\"#STATUS\").setValue(\"3\") : $(\"#panelAssign\").show();\n     \n     \tif($(\"#assignType\").getValue() == \"TO\") {\n       \t\t$(\"#STATUS\").show();\n   \t\t\t$(\"#CLOSE_RSN\").show();\n     \t}\n  \t\t\n   \t\t$(\"#panelRcpts\").hide();\n        panelDisplay(\"panelAssignee\");\n      \t$(\"#chgrpAssignBpmsTo\").hide();\n      \t$(\"#chgrpAssignBpmsCc\").hide();\n     \t$(\"#COMMENTS\").hide();\n      \t$(\"#COMMENTS_NEW\").hide();\n        break;\n        \n   case \"4\": //διαβίβαση σε άλλη υπηρεσία\n        $(\"#STATUS\").hide();\n   \t\t$(\"#CLOSE_RSN\").hide();\n        $(\"#STATUS\").setValue(\"\");\n\t\t$(\"#CLOSE_RSN\").setValue(\"\");\n   \t\t$(\"#STATUS_label\").setValue(\"\");\n        $(\"#panelRcpts\").hide();\n    \n      \t$(\"#panelAssignee\").hide();\n      \t$(\"#chgrpAssignBpmsTo\").hide();\n      \t$(\"#chgrpAssignBpmsCc\").hide();\n      \t$(\"#COMMENTS\").hide();\n      \t$(\"#COMMENTS_NEW\").hide();\n        break;\n        \n    default:\n        \n\t}\n  \n  $(\"#submit1\").setValue($(\".pmdynaform-control-radio[value='\"+s+\"']\").next().html());\n  $(\"#form\\\\[submit1\\\\]\").css(\"background-color\",\"#DAF7A6\");\n}\n//\n//\n// COMMENTS management\n$(\"#form\\\\[COMMENTS_NEW\\\\]\").click(function(){\n  $(\"#form\\\\[COMMENTS_NEW\\\\]\").attr(\"rows\", \"11\");\n});\n$(\"#form\\\\[COMMENTS\\\\]\").click(function(){\n  $(\"#form\\\\[COMMENTS\\\\]\").attr(\"rows\", \"11\");\n});\n$(\"#form\\\\[COMMENTS_NEW\\\\]\").focusout(function(){\n  $(\"#form\\\\[COMMENTS_NEW\\\\]\").attr(\"rows\", \"2\");\n});\n$(\"#form\\\\[COMMENTS\\\\]\").focusout(function(){\n  $(\"#form\\\\[COMMENTS\\\\]\").attr(\"rows\", \"2\");\n});\n\n/*\n*\n*\n* CheckGroup Assign managment\n*\n*/\nfunction chgrpsDelEqls(chgrpSet,chgrpUnset, newSetValue) {\n  \n  \tvar chgrpUnsetValues = $(\"#\"+chgrpUnset).getValue();\n  \n//    $(\"#\"+chgrpSet).getValue().forEach(function(i, item){$(\"#form\\\\[\"+chgrpUnset+\"\\\\]\\\\[\"+i+\"\\\\]\").removeAttr('checked')});\n  \t\n  $(\"#\"+chgrpSet).getValue().forEach(function(i, item){\n    \n      \tchgrpUnsetValues = $.grep(chgrpUnsetValues, function(j, jtem){\n        \n        \t\t\t\t\t\t\treturn(i !== j);\n      \t\n      \t\t\t\t\t\t\t});\n\n  });\n  \n  $(\"#\"+chgrpUnset).setValue(chgrpUnsetValues);\n\n  if($(\"#\"+chgrpSet).attr('id').indexOf('chgrpBpms') > -1 ) { //checkgrpoup των Διευθύνσεων πρωτοκόλλησης\n  \tsCount = $(\"#\"+chgrpSet).getValue().length + $(\"#\"+chgrpUnset).getValue().length;\n  \t$(\"#countRRcpts\").setValue(sCount);\n  }\n}\n\n\n\n// τρικ για διόρθωμα των τιμών στο grid.  \n$(window).ready(function(){\n  \n $(\"#form\\\\[chgrpAssignBpmsCc\\\\]\\\\[all\\\\]\").next(\"span\").html(\"<u>Επιλογή όλων</u>\");\n $(\"#form\\\\[chgrpAssignBpmsTo\\\\]\\\\[all\\\\]\").next(\"span\").html(\"<u>Επιλογή όλων</u>\");\n  \n// set events for select all option  \n  $(\"#form\\\\[chgrpAssignBpmsCc\\\\]\\\\[all\\\\]\").click(function () { // add event for ALL element  \n    $(\"#chgrpAssignBpmsCc\").find(\"input:checkbox\").not(this).prop('checked', this.checked);\n \t  });\n  \n  $(\"#form\\\\[chgrpAssignBpmsTo\\\\]\\\\[all\\\\]\").click(function () { // add event for ALL element  \n    $(\"#chgrpAssignBpmsTo\").find(\"input:checkbox\").not(this).prop('checked', this.checked);\n \t  });\n//  \n  $(\"#chgrpAssignBpmsCc\").setOnchange(function(newValue){    //CC option\n  if(newValue.length > 0) chgrpsDelEqls('chgrpAssignBpmsCc','chgrpAssignBpmsTo', newValue )\n   \n  });\n  \n  $(\"#chgrpAssignBpmsTo\").setOnchange(function(newValue){    //CC option\n  if(newValue.length > 0) chgrpsDelEqls('chgrpAssignBpmsTo','chgrpAssignBpmsCc', newValue )\n   \n  });\n\n//\n  $(\"#STATUS\").setValue( $(\"#statusValue\").getValue());\n     var s= $(\"#assignment\").getValue();\n\tselectAssign(s);                                 \n                                      \n});"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title1","label":" Ανάθεση εισερχομένου εγγράφου.\n","colSpan":12}],[{"type":"textarea","variable":"SUBJ","var_uid":"81130981457ab42b6c29611041528511","dataType":"string","protectedValue":false,"id":"SUBJ","name":"SUBJ","label":"Θέμα","defaultValue":"","placeholder":"","hint":"","required":false,"validate":"","validateMessage":"","mode":"disabled","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","rows":"3","var_name":"SUBJ","colSpan":6},{"type":"text","variable":"PROT_ID_STR","var_uid":"61087036157ab42b6b2c250076316161","dataType":"string","protectedValue":false,"id":"PROT_ID_STR","name":"PROT_ID_STR","label":"Αρ.  Πρωτ.","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":100